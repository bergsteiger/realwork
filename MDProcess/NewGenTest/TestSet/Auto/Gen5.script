Test Gen
 
 : OUTPRIM IN %S
  %SN .
  %SS .
  %S %ST %ST |N .
 ;
 
 : OUTSAFE IN %S
  %S NotValid ? EXIT
  %S OUTPRIM
 ;
 
 : OUTSAFEGEN IN %S
  %S NotValid ? EXIT
  %S%G ==> ( IN %S %S OUTPRIM %S %G ==> ( @SELF DO ) )
 ;
 
 : OUT IN %S
  %S OUTPRIM
