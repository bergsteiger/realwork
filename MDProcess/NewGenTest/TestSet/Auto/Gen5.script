  %S%G ==> OUTPRIM
  %S%Attrs ==> ( @SELF DO )
  %S%Ops ==> ( @SELF DO )
  %S%C ==> ( @SELF DO )
  %S%T OUTSAFE
  %S%T OUTSAFEGEN
  %S%Lnks ==> ( IN %S %S OUTSAFE %S %T OUTSAFE )
 ;
 
 //@ "Shared Delphi" %C ==> OUT
 @ F1 OUT
