USES
    QuickTestsUtils.script
;

PROCEDURE "Заполнение атрибутов для исходящих органов"
	[ '#21' ] >>> AttrNumbersArray
;

PROCEDURE "Заполнение атрибутов для исходящих органов 1"
	[ '#539' ] >>> AttrNumbersArray
;

CONST FILL_PARAM 0
CONST CLEAR_PARAM 1

Тест TK224789398	
	BOOLEAN VAR Вечно
	ДА >>> Вечно 
	INTEGER VAR l_WorkState
	FILL_PARAM >>> l_WorkState
    ПОКА Вечно (
		Параметры: (
			"Документ для тестирования атрибутов"
			"Работа с атрибутами" 
			"Закрывать все окна"
		)
		Выполнить (			
			Если ( l_WorkState РАВНО FILL_PARAM ) то
			(
				"Первый эталон дерева"
				TRY
					Выполнить_здесь ( "Проверка для дерева атрибутов" )
				FINALLY
					"Очистить атрибут Исходящие органы"
					"Заполнение атрибутов для исходящих органов 1"
					"Изменить атрибут Исходящие органы"
					"Сохранить документ"
				END
				CLEAR_PARAM >>> l_WorkState
			)
			иначе
			(
				"Второй эталон дерева"
				TRY
					Выполнить_здесь ( "Проверка для дерева атрибутов" )
				FINALLY
					"Очистить атрибут Исходящие органы"
					"Заполнение атрибутов для исходящих органов"
					"Изменить атрибут Исходящие органы"
					"Сохранить документ"
				END
				FILL_PARAM >>> l_WorkState
			)
		)
		"Обработать сообщения"
		Если test:ShouldStop то 
		(
			"Очистка тестовой базы"
			выходим
		)
	)	
;

TK224789398
