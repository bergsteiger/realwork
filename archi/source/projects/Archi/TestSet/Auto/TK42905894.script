// Появлялось ошибка в смещении 17-й ссылки с параграфа 1.7, на предыдущий параграф с номером 1. 
// При импорте документа болше не проявлялась.
USES
	QuickTestsUtils.script
;

Тест TK42905894
	Параметры: ( 
				"Документ без меток" 
				"Проверка EVD документа"
				"Очистка тестовой базы" 
				)
	Выполнить ( 
				CONST cnSubCount 10
	
				INTEGER VAR l_SubNumber
				1 >>> l_SubNumber
				cnSubCount раз (
					l_SubNumber CASE						
					 1 ( 17 раз "Нажать вниз" )
					 2 ( "В конец документа" "Обработать сообщения" 14 раз "Нажать вверх" )
					 3 ( 4 раза "Нажать вниз" )
					 4 ( 11 >>> l_SubNumber "В начало документа" "Обработать сообщения" 22 раза "Нажать вниз"  )
					 12 ( 2 раза "Нажать вниз" )
					 13 ( 3 раза "Нажать вниз" )
					 14 ( 16 раз "Нажать вниз" )
					 15 ( 4 раза "Нажать вниз" )
					 16 ( 13 раз "Нажать вниз" )
					 17 ( 5 раз "Нажать вниз" )
					END			
					"Обработать сообщения"
					"Создать метку с номером {(l_SubNumber)}" 
					1 +! l_SubNumber 
				)
				"Разбивка на блоки"
			)
;

TK42905894