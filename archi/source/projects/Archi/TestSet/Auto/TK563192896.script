USES
	QuickTestsUtils.script
;

PROCEDURE "Вызывать список открытых окон"
	'Alt+F1' key		
;

PROCEDURE "Вызвать динамическое меню и нажать кнопку 'Перейти'"
	OBJECT VAR l_Control
	"Контрол по имени {('lstWinSet')}" >>> l_Control
	"Установить курсор мыши по координатам {(15 15)} относительно контрола {(l_Control)}" 
	THREAD ( 500 SLEEP 	
		"Установить курсор мыши по координатам {(20 20)} относительно контрола {(l_Control)}" 		
		"Клик левой кнопкой мыши"
	)
 	"Клик правой кнопкой мыши"
;

Тест TK563192896
	Параметры: ( "Не открывать документ" )
	Выполнить ( 
		@ ( "Вызывать список открытых окон" ) MODAL (
			"Вызвать динамическое меню и нажать кнопку 'Перейти'"
		)
	)
;

TK563192896
