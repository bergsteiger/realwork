USES	
	ArchiControls.script
	MainFormUtils.script
	QuickTestsUtils.script
;

PROCEDURE "Фокус на словарь 'Исходящие органы'"
	"Установить фокус в контрол с именем" "Cловарь 'Исходящие органы'"
;

PROCEDURE "Фокус на перегруженные элементы"
	"Установить фокус в контрол с именем" "Перегруженные элементы"
;

PROCEDURE "Вызвать динамическое меню кликом ПКМ и выбрать команду 'Запомнить'"
	"Установить курсор мыши по координатам {(220 220)} относительно контрола {("Контрол в фокусе")}"
	THREAD ( 
			500 SLEEP 	
			"Установить курсор мыши по координатам {(230 230)} относительно контрола {("Контрол в фокусе")}" 		
			"Клик левой кнопкой мыши"		
	)
	"Клик правой кнопкой мыши"
;

Тест TK563513310 
	Параметры: (
		"Не открывать документ" 
		"Восстанавливать позицию мыши"
		"Закрывать все окна"
	) 
	Выполнить (	
		"Вызвать из главного меню {('Словари/Исходящие органы')}"
		"Развернуть окно" 
		"Фокус на словарь 'Исходящие органы'"
		"Вызвать из меню дочернего окна {('Статистика/"Перегруженные" элементы')}"
		"Развернуть окно"
		"Фокус на перегруженные элементы"
		"Вызвать динамическое меню кликом ПКМ и выбрать команду 'Запомнить'"
	)
;

