//  Установка нескольких гиперссылок на одну метку (на вкладке Справка)
USES
	QuickTestsUtils.script
;

Тест TK628100198_126
	Параметры: (
		"Просто удобный документ"
		"Работа со справкой"
		"Проверка EVD документа"
		"Закрывать все открытые окна"
		"Восстанавливать позицию мыши"
	) 
	Выполнить (
		TRY
			"Установить курсор над меткой с номером, вызвать динамическое меню и выбрать команду 'Запомнить' на вкладке Справка"
			"Вниз и выделить слово 'Зарегистрировано' на вкладке Справка и установить курсор"	
			"Нажать 'Alt+Shift+ЛМ'"
			"Вправо и выделить слово 'Минюсте' на вкладке Справка и установить курсор"
			"Нажать 'Alt+Shift+ЛМ'"
		FINALLY	
			"Отказаться от простановки ссылки"
		END
	)
;

TK628100198_126