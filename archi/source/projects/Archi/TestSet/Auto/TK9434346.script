USES
	QuickTestsUtils.script
;

Тест TK9434346
	INTEGER VAR l_NewHeight	
	600 >>> l_NewHeight 
	
	WHILE ( l_NewHeight 1100 LESS ) 
	(
		TRY
			Параметры: ( 
						"Документ для поиска Assert при прокрукте"	
						"Ширина {(1400)} и высота{(l_NewHeight)}, спецсимволы, реестр" 
			)
			Выполнить ( 				
				"Прокрутить документ вниз до конца стрелкой вниз"
			)
			l_NewHeight 20 + >>> l_NewHeight			
		FINALLY
			l_NewHeight .
			"Закрыть без сохранения"
		END	
	)
;

TK9434346