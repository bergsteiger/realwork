INTEGER VAR g_MoveCount
INTEGER VAR g_PageCount

PROCEDURE "Курсор вниз на строки" INTEGER IN aCount
		cnMoveCursorDown >>> g_StartAction
		aCount >>> g_MoveCount
		0 >>> g_PageCount
;

PROCEDURE "Прокрутка вниз на строк" INTEGER IN aCount
		cnScrollDown >>> g_StartAction
		aCount >>> g_MoveCount	
		0 >>> g_PageCount
;

INTEGER VAR g_TabCount

PROCEDURE "Сразу выполнять тест"
	cnNoStartAction >>> g_StartAction
	0 >>> g_MoveCount
	0 >>> g_PageCount
	0 >>> g_TabCount
;

PROCEDURE "Курсор вниз на и в начало строки" INTEGER IN aCount
		cnMoveCursorDownAndHome >>> g_StartAction
		aCount >>> g_MoveCount
		0 >>> g_PageCount
		0 >>> g_TabCount
;

PROCEDURE "На страницы вниз и сместить курсор вниз на строки" INTEGER IN aPageCount INTEGER IN aCount
		cnPageDownAndMoveCursorDown >>> g_StartAction
		aCount >>> g_MoveCount
		aPageCount >>> g_PageCount
;

PROCEDURE "На страницы вниз и сместить курсор вверх на строки" INTEGER IN aPageCount INTEGER IN aCount
		cnPageDownAndMoveCursorUp >>> g_StartAction
		aCount >>> g_MoveCount
		aPageCount >>> g_PageCount
		0 >>> g_TabCount
;

PROCEDURE "Курсор вниз на строк и нажать табуляцию" INTEGER IN aMoveCount INTEGER IN aTabCount
	cnMoveCursorDownAndTab >>> g_StartAction
	aMoveCount >>> g_MoveCount
	aTabCount >>> g_TabCount
;

PROCEDURE "Нажать табуляцию и в конец строки" INTEGER IN aTabCount
		cnTabAndEnd >>> g_StartAction
		0 >>> g_MoveCount
		0 >>> g_PageCount
		aTabCount >>> g_TabCount	
;

