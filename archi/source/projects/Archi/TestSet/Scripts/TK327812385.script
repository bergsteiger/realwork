USES
		QuickTestsUtils.script
;

PROCEDURE "Внести изменения"
	3 раз "Нажать вниз"
	"Набить текст {('изменение')}"
;

Тест TK327812385 
	Параметры: ( "Документ для тестирования предыдущей версии" )
	Выполнить (  
				"Внести изменения"
				"Сохранить документ"
				wait:Ok
				TRY
					"Вернуться к сохраненной версии"
				FINALLY
					waited:? ASSERT
				END 
			) 
;

TK327812385