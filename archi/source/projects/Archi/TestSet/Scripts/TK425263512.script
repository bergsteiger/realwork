USES
	QuickTestsUtils.script
;

OBJECT VAR l_Editor

PROCEDURE "Начальное позиционирование"
	21 раз "Нажать вниз"
	2 раз "Нажать вверх"
	"Нажать табуляцию"
	"Нажать вниз"
	4 раз "Нажать табуляцию"
	"В конец строки"
;

PROCEDURE "Разделить и сдвинуть курсор вниз"
	"Разделить ячейку по вертикали {(l_Editor)}"
	"Нажать вниз"
	"В конец строки"
;

PROCEDURE "Разделить блок ячеек"
    4 раза "Разделить и сдвинуть курсор вниз" 
;

PROCEDURE "Скопировать ширину и применить к той же таблице"	
	"Начать выделение"
	3 раз "Нажать вниз"
	"В конец строки"
	"Закончить выделение"
	"Запомнить ширины ячеек"	
	"Применить ширины ячеек"
;

PROCEDURE "Сместиться на первую"
	2 раза "Нажать вверх"
	"Нажать вправо"
	"Нажать вниз"
	4 раз "Нажать табуляцию"
;

PROCEDURE "Переместиться на следующий блок ячеек"
	5 раз "Нажать вниз"
	"Нажать вверх"
	2 раз "Нажать влево"
;

PROCEDURE "Объеденить два столбца"
	4 раза "Нажать вверх"
	2 раза "Нажать табуляцию"
	"Начать выделение"
	"Нажать табуляцию"
	3 раза "Нажать вниз"
	"В конец строки"
	"Закончить выделение"	
	"Объединить ячейки"
;

PROCEDURE "Сместиться на первую во втором блоке"
	"Нажать вверх"
	3 раза "Нажать влево"
	"В начало строки"
;

Тест TK425263512
	Параметры: ( "Проверка EVD документа" )
	Выполнить (
					focused:control:push >>> l_Editor	
					"Начальное позиционирование"
					"Разделить блок ячеек"	
					"Сместиться на первую"
					"Скопировать ширину и применить к той же таблице"
					"Объеденить два столбца"			
			)
;

TK425263512