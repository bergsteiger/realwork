MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = DbUpdate64.exe DbUpdate.exe
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

DbUpdate64.exe: DbUpdate64.dpr
  $(DCC)

DbUpdate.exe: DbUpdate.dpr
  $(DCC)


