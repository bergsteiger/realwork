    }
    : = {
     Text = 'учебная работа обучающегося в классе, отношение к учебным дисциплинам и склонности к определенным предметам;'
    }
    : = {
     Text = 'зарождение и развитие интереса к обучению в определенном военном образовательном учреждении и к получению офицерской профессии;'
    }
    : = {
     Text = 'навыки в выполнении распорядка дня, поддержании внутреннего порядка, несении службы внутреннего наряда;'
    }
    : = {
     Text = 'режим работы и отдыха;'
    }
    : = {
     Text = 'участие в общественно полезном труде;'
