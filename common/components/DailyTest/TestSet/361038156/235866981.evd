        Finish = 22
        : = {
         DocID = 5884566
        }
       }
      }
     }
     FirstIndent = 139
     Text = 'См. графический объект'
    }
    : = {
     Style = -9
     Text = ''
    }
    : = {
     Handle = 1073744830
     LeftIndent = 139
     Subs = {
      Children = 2
      : = {
       Handle = Sub
       : = {
        Handle = 852
       }
      }
     }
     Style = -9
     Segments = {
      : = {
       Handle = Hyperlinks
       : HyperLink = {
        Start = 5
        Finish = 22
        : = {
         DocID = 5884567
        }
       }
      }
     }
     FirstIndent = 139
     Text = 'См. графический объект'
    }
    : = {
     Style = -9
     Text = ''
    }
    : = {
     Handle = 3007
     Style = -1
     Text = 'Анализ полученных результатов показал, что средняя неравномерность газопотребления по Москве за рассмотренные 7 последних лет колеблется:'
    }
    : = {
     Handle = 3008
     Style = -1
     Text = '- в зимний период от 1,23 до 1,45;'
    }
    : = {
     Handle = 3009
     Style = -1
     Text = '- в летний период от 0,55 до 0,7.'
    }
    : = {
     Handle = 3010
     Style = -1
     Text = 'Среднее значение неравномерности составляет 1,4 в январе и феврале, 0,6 в июле.'
    }
    : = {
     Handle = 3011
     Style = -1
     Text = 'Максимальная величина неравномерности составила 1,45 в остальные годы она не превышает 1,38 - 1,4.'
    }
    : = {
     Handle = 3012
     Style = -1
     Text = 'По Московскому региону (Москва и Московская область) наивысший уровень газопотребления отмечен в зимние месяцы: январь, февраль, декабрь. Наиболее холодными были зимы 2002/2003 и 2005/2006 гг., о чем свидетельствуют как пиковые, так и средние температуры.'
    }
    : = {
     Handle = 3013
     Style = -1
     Text = 'По результатам анализа можно констатировать, что пики минимума температур приходятся на январь и февраль месяцы. Так, в январе 2006 г. температурная пика составила - 28°С, а в феврале - 26,5°С. При этом температура воздуха ниже -25°С продержалась более 5 суток два раза в течение месяца.'
    }
    : = {
     Handle = 3014
     Style = -1
     Text = 'Проведенный анализ влияния температур на объемы потребления газа показывает, что в месяцы низких температур увеличивается сезонная неравномерность. Поэтому обеспечение природным газом потребителей в периоды с температурой ниже средних значений должно быть реализовано за счет использования дополнительно резервных видов топлива.'
    }
    : = {
     Handle = 3015
     Style = -1
     Text = 'Справочно: Действующий график перевода потребителей г. Москвы на резервные виды топлива в периоды низких температур предусматривает перевод 42 предприятий, суточное их потребление составляет 1,8 млн. куб. м. в сутки. Планируемый объем замещения составляет около 1,7 млн. куб. м., причем по очереди 1 - 1,4 млн. куб. м. в сутки.'
    }
    : = {
     Handle = 3016
     Style = -1
     Text = 'В настоящее время 12 ТЭЦ имеют резервное топливо для покрытия пикового спроса в зимний период (зимой 2006/2007 года таких объектов было 10.). Высвобождаемый объем составляет от 10 до 20% суточного потребления в зависимости от вводимой очереди ограничений.'
    }
   }
