      Text = '- РТС и КТС ОАО "МОЭК": РТС "Бабушкин-1, и 2", "Переяславская", "Новомосковская", КТС-44, КТС-8, КТС-48, КТС "СВС" (СВАО); КТС-28, КТС-42, КТС "Косинская" (ВАО), РТС "Фрезер" (ЮВАО), "Ленино-Дачное", "Нагатино" (ЮАО); РТС "Рублево", "Матвеевская", КТС-11, КТС-24, КТС-25, КТС-26 (ЗАО); "Тушино-1, -2, -3, -4" (СЗАО).'
     }
     : = {
      Handle = 393
      Style = -1
      Segments = {
       : = {
        Handle = Objects
        Children = 3
        : ObjectSegment = {
         Start = 40
         Finish = 40
         : Formula = {
          Text = 'Р<60>=0.3 '
         }
        }
        : ObjectSegment = {
