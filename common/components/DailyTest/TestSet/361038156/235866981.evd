    }
    : = {
     Handle = 3311
     Style = -1
     Text = '- Отсутствие свободных неиспользуемых земель под объекты газоснабжения (магистральные газопроводы, ГРС, КРП и др.)'
    }
    : = {
     Handle = 3312
     Style = -1
     Text = '- Обеспечение требований охранных зон для объектов газоснабжения.'
    }
    : = {
     Handle = 3313
     Style = -1
     Text = '- Много собственников земель, интересы (требования) которых следует учитывать при реализации проектов.'
    }
   }
   : Block = {
    Handle = 29018
    ShortName = 'Пункт 18'
    InContents = ev_spInContentsAndOnSubPanel
    Children = 2
    : = {
     Handle = 3314
     Style = -1
     Text = '18. Предлагаемые выше технические решения будут рассмотрены при ежегодной актуализации схемы и при ее корректировке на предстоящий период с участием ГУП "Мосгаз", ГУП МО "Мособлгаз", ОАО "Газпром трансгаз Москва", ОАО "Межрегионгаз" и ОАО "Газпром промгаз").'
    }
    : = {
     Handle = 3315
     Style = -1
     Segments = {
      : = {
       Handle = Hyperlinks
