   : Block = {
    Handle = 243
    ShortName = '4.3. Оценка надежности существующей системы газоснабжения региона'
    InContents = ev_spInContentsAndOnSubPanel
    Children = 6
    : = {
     Handle = 2782
     Style = -3
     Text = '4.3. Оценка надежности существующей системы газоснабжения региона'
    }
    : = {
     Text = ''
    }
    : = {
     Handle = 2783
     Style = -1
     Text = 'Исследование надежности системы проводилось на расчетной схеме газоснабжения Московского региона. Проведен ряд расчетов, в рамках которых моделировалось аварийная ситуация на участках КГМО, связанная с выходом из рабочего состояния одного из участков кольцевого газопровода.'
    }
    : = {
     Handle = 2784
     Style = -1
     Text = 'В случае возникновения аварий на участках кольцевого газопровода, происходит вывод его из эксплуатации и перераспределение потоков между кольцевыми газопроводами по перемычкам.'
    }
    : = {
     Handle = 2785
     Style = -1
     Text = 'В качестве аварийных были рассмотрены внутренние (эндогенные) - выход из строя участков на ответственных направлениях и внешние (экзогенные) - снижение давления на главных вводах системы. К примеру, был выведен из работы участок магистрали КС Воскресенск - КС Яхрома нитка КГМО-I 170-205 км, при этом потери общего потребления составили 3,2%.'
    }
    : = {
     Handle = 2786
     Style = -1
     Text = 'Все последующие расчеты также показали, что при возникновении аварийных ситуаций на участках КГМО система вполне удовлетворяет необходимым требованиям надежности.'
    }
   }
  }
