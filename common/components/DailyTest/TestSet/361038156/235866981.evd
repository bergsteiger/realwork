    ShortName = '1.1. Общая характеристика газового хозяйства г. Москвы'
    InContents = ev_spInContentsAndOnSubPanel
    Children = 12
    : = {
     Handle = 145
     Style = -3
     Text = '1.1. Общая характеристика газового хозяйства города Москвы'
    }
    : = {
     Text = ''
    }
    : = {
     Handle = 146
     Style = -1
     Text = 'Город Москва включен в Единую систему газоснабжения (ЕГС) России и является одним из самых крупных потребителей природного газа в стране.'
    }
    : = {
     Handle = 147
     Style = -1
     Text = 'Столица России характеризуется развитой энергетической отраслью и значительной плотностью населения. Это обуславливает высокий уровень потребления газа, как топлива, обладающего высокой калорийностью, обеспечивающего по сравнению с другими видами органического топлива более высокий КПД. Использование природного газа сокращает отходы производства и повышает экономический эффект во многих отраслях промышленности, уменьшает загрязненность воздушного бассейна, улучшает условия труда.'
    }
    : = {
     Handle = 148
     Style = -1
     Text = 'По протяженности газораспределительной сети и количеству сооружений на ней, а также числу газифицированных объектов газовое хозяйство города Москвы относится к числу крупнейших газовых хозяйств всей Российской Федерации.'
    }
    : = {
     Handle = 149
     Style = -1
     Text = 'Городская система газоснабжения и газораспределения представляет собой радиально-кольцевую и многоступенчатую, разветвленную сеть газопроводов и газораспределительных пунктов с давлениями в радиальных газопроводах 1,2 и 0,6 МПа и в кольцевых газопроводах 0,3 и 0,1 МПа.'
    }
    : = {
     Handle = 150
     Style = -1
     Text = 'Газовое хозяйство города Москвы, удельный вес природного газа в топливном балансе которого превышает 92%, играет исключительную роль в обеспечении жизнедеятельности столицы. Общая протяженность газовых сетей столицы, по состоянию на начало 2009 года, составляет порядка 7900 км, более 3900 км из них проложены подземно. Протяженность газопроводов высокого и среднего давления равняется в 1400 км.'
    }
    : = {
     Handle = 151
     Style = -1
     Text = 'В настоящее время природный газ, поступающий в город Москву из других регионов России по магистральным газопроводам, используется:'
    }
    : = {
     Handle = 152
     Style = -1
     Text = '- в качестве топлива для производства тепловой и электрической энергии на городских ТЭЦ, РТС, КТС, газотурбинных установках и промышленно-производственных котельных;'
    }
    : = {
     Handle = 153
     Style = -1
     Text = '- в промышленных установках (печах, сушильных камерах и т.п.);'
    }
    : = {
     Handle = 154
     Style = -1
     Text = '- в качестве моторного топлива на АГНКС и АГНС;'
    }
    : = {
     Handle = 155
     Style = -1
     Text = '- для отопления и горячего водоснабжения от индивидуальных установок выработки тепла и приготовления пищи на газовых плитах.'
    }
