 }
 : = {
  Style = -15
  Text = '«Статья 8. Служебное удостоверение и нагрудный знак депутата Совета'
 }
 : = {
  Text = 'Депутат Совета имеет служебное удостоверение и нагрудный знак депутата Совета, которыми он пользуется в течение срока своих полномочий.'
 }
 : = {
  Text = 'Служебное удостоверение выдается депутату Совета в порядке, установленном Президентом Республики Беларусь.'
 }
 : = {
  Text = 'Президентом Республики Беларусь учреждаются нагрудные знаки депутатов Советов и утверждаются положения о них.».'
