  Text = 'Часть 4 статьи 444 - в редакции Закона Республики Беларусь от 27 декабря 2010 г. № 223-З'
 }
 : = {
  Style = -15
  Text = 'Статья 445. Назначение судебного разбирательства'
 }
 : = {
  Style = -9
  Segments = {
   : = {
    Handle = Hyperlinks
    : HyperLink = {
     Start = 49
     Finish = 73
     : = {
      DocID = 79014470
     }
    }
