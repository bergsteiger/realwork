 : = {
  Style = -3
  Text = 'ГЛАВА 38<10>ПОСТАНОВЛЕНИЕ ПРИГОВОРА'
 }
 : = {
  Style = -15
  Text = 'Статья 349. Постановление приговора именем Республики Беларусь'
 }
 : = {
  Style = -9
  Segments = {
   : = {
