     : = {
      DocID = 79180089
     }
    }
   }
  }
  Text = 'Часть 4 статьи 184 - в редакции Закона Республики Беларусь от 15 июля 2015 г. № 307-З'
 }
 : = {
  Style = -9
  Segments = {
   : = {
    Handle = Hyperlinks
    : HyperLink = {
     Start = 33
