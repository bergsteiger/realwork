 }
 : = {
  Style = -15
  Text = 'Статья 160. Дополнительные поощрительные отпуска'
 }
 : = {
  Text = 'Дополнительные поощрительные отпуска могут устанавливаться коллективным договором, соглашением или нанимателем всем работникам, отдельным их категориям (по специальностям и видам производств, работ, структурных подразделений), а персонально – трудовым договором.'
 }
 : = {
  Style = -9
  Segments = {
