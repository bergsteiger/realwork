  Text = 'Пункт 1 статьи 232 - в редакции Закона Республики Беларусь от 20 июля 2007 г. № 272-З'
 }
 : = {
  Text = '2) выполнять нормы и обязательства по охране труда, предусмотренные коллективным договором, соглашением, трудовым договором, должностными обязанностями и правилами внутреннего трудового распорядка;'
 }
 : = {
  Style = -9
  Segments = {
