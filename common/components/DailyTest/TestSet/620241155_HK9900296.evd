 : = {
  Style = -15
  Text = 'Статья 136. Выходные дни'
 }
 : = {
  Text = 'Всем работникам предоставляются выходные дни (еженедельный непрерывный отдых).'
 }
 : = {
  Text = 'При пятидневной рабочей неделе предоставляются два выходных дня каждую календарную неделю.'
 }
 : = {
  Style = -9
  Segments = {
   : = {
