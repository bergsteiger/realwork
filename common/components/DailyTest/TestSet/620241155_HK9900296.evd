  Text = 'Статья 57 - в редакции Закона Республики Беларусь от 20 июля 2007 г. № 272-З'
 }
 : = {
  Style = -15
  Text = 'Статья 58. Индексация заработной платы'
 }
 : = {
  Text = 'Индексация заработной платы осуществляется в связи с инфляцией, а также при несвоевременной ее выплате в порядке и на условиях, предусмотренных законодательством.'
 }
 : = {
  Style = -9
  Segments = {
