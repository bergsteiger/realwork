  Text = 'Статья 178 - в редакции Закона Республики Беларусь от 20 июля 2007 г. № 272-З'
 }
 : = {
  Style = -15
  Text = 'Статья 179. Денежная компенсация за неиспользованный трудовой отпуск при увольнении'
 }
 : = {
  Text = 'При увольнении независимо от его основания работнику, который не использовал или использовал не полностью трудовой отпуск, выплачивается денежная компенсация.'
 }
 : = {
  Text = 'Денежная компенсация за полный трудовой отпуск выплачивается, если ко дню увольнения работник проработал весь рабочий год (12 месяцев минус суммарная продолжительность трудового отпуска, на которую работник имеет право).'
 }
 : = {
  Text = 'Если ко дню увольнения работник проработал часть рабочего года, денежная компенсация выплачивается пропорционально отработанному времени.'
 }
 : = {
  Text = 'Денежная компенсация за неиспользованный трудовой отпуск при увольнении выплачивается в день увольнения.'
 }
