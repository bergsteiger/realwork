  Text = 'Статья 375 - в редакции Закона Республики Беларусь от 8 января 2014 г. № 131-З'
 }
 : = {
  Style = -15
  Text = 'Статья 376. Ответственность сторон за неисполнение норм настоящей главы, коллективного договора, соглашения'
 }
 : = {
  Text = 'За неисполнение норм настоящей главы и обязательств, предусмотренных коллективным договором, соглашением, стороны несут ответственность в соответствии с законодательством, коллективным договором, соглашением.'
 }
 : = {
  Style = -3
  Text = 'Глава 36<10>Разрешение коллективных трудовых споров'
 }
 : = {
  Style = -15
  Text = 'Статья 377. Коллективный трудовой спор'
 }
 : = {
  Text = 'Коллективный трудовой спор – неурегулированные разногласия сторон коллективных трудовых отношений по поводу установления, изменения социально-экономических условий труда и быта работников, заключения, изменения, дополнения, исполнения либо прекращения коллективных договоров, соглашений.'
 }
 : = {
  Style = -9
  Segments = {
   : = {
    Handle = Hyperlinks
    : HyperLink = {
