     Finish = 63
     : = {
      DocID = 79148042
     }
    }
   }
  }
  Text = 'Часть первая статьи 255 - в редакции Закона Республики Беларусь от 8 января 2014 г. № 131-З'
 }
 : = {
  Style = -9
  Segments = {
   : = {
    Handle = Hyperlinks
