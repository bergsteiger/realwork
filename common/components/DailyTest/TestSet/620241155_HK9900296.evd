 }
 : = {
  Text = '4) размеры увеличения оплаты труда за работу в условиях, отличающихся от нормативных;'
 }
 : = {
  Text = '5) меры по поддержанию уровня реального содержания заработной платы, индексации заработной платы;'
 }
 : = {
  Text = '6) ограничения размеров удержаний из заработной платы;'
 }
