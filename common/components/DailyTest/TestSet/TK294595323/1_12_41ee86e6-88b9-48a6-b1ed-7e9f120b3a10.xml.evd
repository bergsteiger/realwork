 : = {
  Text = 'Арбитражный суд Волгоградской области в составе: судьи:      Субы В.Д. ,'
 }
 : = {
  Text = 'с ведением протокола судебного заседания: судьей Субой В.Д.'
 }
 : = {
  Text = 'рассмотрев в судебном заседании  в помещении арбитражного суда Волгоградской области по адресу: 400005, г.Волгоград, ул.7-я Гвардейская, д. 2,  каб. 583 ходатайство  ООО «МОСКВА»  и ООО «Техцентр»  о назначении  судебной  автотехнической экспертизы экспертизы  по делу  возбужденному по  иску ООО «МОСКВА» к   ООО «Техцентр» при участии третьего лица ООО «ЗУБР»  о  расторжении договора  поставки  техники и  взыскании  уплаченной за  товар денежной  суммы.'
 }
 : = {
  Text = 'при участии  представителей:'
 }
 : = {
  Text = 'от  истца: '
 }
 : = {
  LeftIndent = 720
  FirstIndent = 360
  FirstLineIndent = -360
  Justification = ev_itWidth
  Text = '1. Котова Т.А., доверенность №12 от 12.01.09г.'
 }
 : = {
