  Text = 'ПОСТАНОВЛЕНИЕ'
 }
 : = {
  Text = 'апелляционной инстанции по проверке законности и'
 }
 : = {
  Text = 'обоснованности решений арбитражного суда,'
 }
 : = {
  Text = 'не вступивших в законную силу'
 }
 : = {
  Text = ''
 }
 : = {
  Text = '15 ноября 2011 года                                                                                       Дело № А72-82/2005'
 }
