 : = {
  Text = 'ДВЕНАДЦАТЫЙ  АРБИТРАЖНЫЙ АПЕЛЛЯЦИОННЫЙ СУД'
 }
 : = {
  Text = '410031, Россия, г. Саратов, ул. Первомайская, д. 74'
 }
 : = {
  Text = '==================================================================='
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'ПОСТАНОВЛЕНИЕ '
 }
 : = {
  Text = 'г. Саратов                                                                                 Дело № А57-10155/2010'
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'Резолютивная часть постановления объявлена «02» ноября  2011 года'
 }
 : = {
  Text = 'Полный текст постановления изготовлен          «09» ноября 2011 года'
 }
 : = {
  Text = 'Двенадцатый арбитражный апелляционный суд в составе:'
 }
 : = {
  Text = 'председательствующего  судьи О.И. Антоновой,  '
 }
 : = {
  Text = 'судей А.Н. Бирченко, Ф.И. Тимаева'
 }
 : = {
  Text = 'при ведении протокола секретарем судебного заседания Макарихиной О.А.  '
 }
 : = {
  Text = 'рассмотрев  в открытом судебном заседании апелляционную жалобу общества с ограниченной ответственностью «Девон-альянс» на определение об отказе в объединении дел в одно производство арбитражного суда Саратовской области от 22 сентября 2011 года по делу № А57-10155/2010, судья Николаева Л.М.,'
 }
 : = {
  Text = 'по исковому заявлению общества с ограниченной ответственностью «Девон-альянс», Саратовская область, п/о Дубки,'
 }
 : = {
  Text = 'к открытому акционерному обществу «Совхоз-Весна», Саратовская область, п/о Дубки,'
 }
 : = {
  Text = 'открытому акционерному обществу «Газпром», г. Москва,'
 }
 : = {
  Text = 'третьи лица: '
 }
 : = {
  Text = 'Управление федеральной регистрационной службы по Саратовской области, г. Саратов,'
 }
 : = {
  Text = 'Комитет по управлению имуществом Саратовской области, г. Саратов,'
 }
 : = {
  Text = 'Администрация Саратовского муниципального района, г. Саратов,'
 }
 : = {
  Text = 'о признании права собственности отсутствующим,'
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'УСТАНОВИЛ:'
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'В Арбитражный суд Саратовской области обратилось Общество с ограниченной ответственностью «Девон-альянс» с исковым заявлением к Открытому акционерному обществу «Совхоз-Весна», Открытому акционерному обществу «Газпром», третье лицо: Управление федеральной регистрационной службы по Саратовской области, Комитет по управлению имуществом Саратовской области, Администрация Саратовского муниципального района о признании права собственности ОАО «Совхоз-Весна» на сооружение - подземный газопровод высокого давления протяженностью 650 п.м., находящийся по адресу: Саратовская область, Саратовский район, Дубковский округ, 6 км Вольского тракта, зарегистрированное Управлением ФРС по Саратовской области 17 января 2008 года, отсутствующим; признании права собственности ОАО «Совхоз-Весна» на сооружение - подземный газопровод низкого давления протяженностью 572 п.м., находящийся по адресу: Саратовская область, Саратовский район, Дубковский округ, 6 км Вольского тракта, зарегистрированное Управлением ФРС по Саратовской области 17 января 2008 года, отсутствующим; признании права собственности ОАО «Совхоз-Весна» на сооружение - благоустройство площадки АГРС, находящееся по адресу: Саратовская область, Саратовский район, Дубковский округ, 6 км Вольского тракта, зарегистрированное Управлением ФРС по Саратовской области 17 января 2008 года, отсутствующим; признании права собственности ОАО «Газпром»» на сооружение -подземный газопровод высокого давления от места врезки в магистральный газопровод Сторожовка-Степное до АГРС ОАО «Совхоз-Весна» протяженностью 650 п.м., находящийся по адресу: Саратовская область, Саратовский район, Дубковский округ, 6 км Вольского тракта, кадастровый или условный номер объекта 64-64-40/138/2007-100 зарегистрированное Управлением ФРС по Саратовской области, отсутствующим; признании права собственности ОАО «Газпром» на сооружение - подземный газопровод низкого давления от АГРС ОАО «Совхоз-Весна» до двухквартирного дома оператора протяженностью 572 п.м., находящийся по адресу: Саратовская область, Саратовский район, Дубковский округ, 6 км Вольского тракта, кадастровый или условный номер объекта 64-64-40/138/2007-091 зарегистрированное Управлением ФРС по Саратовской области, отсутствующим; признании права собственности ОАО «Газпром» на сооружение - благоустройство площадки АГРС, находящийся по адресу: Саратовская область, Саратовский район, Дубковский округ, 6 км Вольского тракта, кадастровый или условный номер объекта 64-64-40/138/2007-086 зарегистрированное Управлением ФРС по Саратовской области, отсутствующим.'
 }
 : = {
  Text = 'В ходе рассмотрения дела судом ООО «Девон-альянс» было заявлено ходатайство об объединении в одно производство дел № А57-10155/2010 и № А57-22498/2008.'
 }
 : = {
  Text = 'Определением Арбитражного суда Саратовской области от 22.09.2011г. в удовлетворении ходатайства отказано, исходя из того, что заявленные требования в рамках дела №А57-22498/2008 и требования, заявленные в рамках настоящего дела не однородны, содержат самостоятельные основания и предметы заявленных требований, различны по предмету доказывания, а также из временного разрыва в подаче исковых заявлений (исковое заявление по делу № А57-22498/2008 подано в суд 21 октября 2008 года, тогда как исковое заявление по настоящему делу подано 05 августа 2010), объема совершенных в рамках настоящего дела процессуальных действий, необходимых для принятия решения по существу спора.'
 }
 : = {
  Text = 'Не согласившись с принятым судебным актом, ООО «Девон-альянс» обратился с апелляционной жалобой на указанное определение суда, в которой просит определение отменить по основаниям, изложенным в жалобе. '
 }
 : = {
  Text = 'Дело рассматривается в апелляционной инстанции в порядке ст. 266, 268 АПК РФ.'
 }
 : = {
  Text = 'Суд апелляционной инстанции, оценив представленные в материалы дела доказательства, доводы апелляционной жалобы, выслушав полномочных представителей лиц, участвующих в деле приходит к выводу о наличии оснований для отмены обжалуемого судебного акта в силу следующего. '
 }
 : = {
  Text = 'Согласно части 2 статьи 130 Арбитражного процессуального кодекса Российской Федерации арбитражный суд первой инстанции вправе объединить несколько однородных дел, в которых участвуют одни и те же лица, в одно производство для совместного рассмотрения.'
 }
 : = {
  Text = 'Частью 2.1 статьи 130 Арбитражного процессуального кодекса Российской Федерации предусмотрено, что арбитражный суд первой инстанции, установив, что в его производстве имеются несколько дел, связанных между собой по основаниям возникновения заявленных требований и (или) представленным доказательствам, а также в иных случаях возникновения риска принятия противоречащих друг другу судебных актов, по собственной инициативе или по ходатайству лица, участвующего в деле, объединяет эти дела в одно производство для их совместного рассмотрения.'
 }
 : = {
  Text = 'Судом апелляционной инстанции установлено, что в производстве Арбитражного суда Саратовской области  находится дело № А57-22498/2008  ООО «Девон-Альянс» к ОАО «Совхоз-Весна», ОАО «Газапром», третьи лица: Комитет по управлению имуществом Саратовской области, ООО «ГЕО-ШТАМП», Администрация Саратовского муниципального района, город Саратов, ФГУ «Земельная кадастровая палата» Саратовской области о прекращении нарушенного права и изъятии земельного участка. Заявленные требования основаны на том, что в 2008г. ООО «Девон-Альянс» стало известно, что объекты недвижимого имущества ОАО «Совхоз-Весна» фактически построены на земельном участке ООО «Девон-Альянс». Такое нарушение права последнего послужило основанием для обращения в суд с иском. '
 }
 : = {
  Text = 'В свою очередь, 05.08.2010г. ООО «Девон-Альянс» обратилось в суд с иском о признании права собственности отсутствующим (дело №А57-10155/2010), при этом указав, что факт нахождения спорных объектов, принадлежащих ответчику на земельном участке истца установлен Постановлением Федерального арбитражного суда Поволжского округа в рамках проверки законности и обоснованности судебных актов первой и апелляционной инстанции. В частности, отменяя предыдущие судебные акты, суд кассационной инстанции указал на то, что в материалах дела имеются доказательства смещения объектов ОАО «Совхоз-Весна». По мнению истца, свидетельствует об отсутствии права собственности на земельный участок под объектами ответчика, а, следовательно отсутствует и право собственности на них. '
 }
 : = {
  Text = 'Изложенное позволяет сделать вывод о том, что основания исков как в рамках дела № А57-22498/2008 так и по делу А57-10155/2010 тождественны, а именно - наличие объектов, построенных  ОАО «Совхоз-Весна» на земельном участке ООО «Девон-альянс». '
 }
 : = {
  Text = ' Закрепленные в ст. 130 Арбитражного процессуального кодекса Российской Федерации положения направлены на обеспечение быстрого и правильного разрешения спора, соответствующего целям эффективности правосудия.'
 }
 : = {
  Text = 'Учитывая, что  дела №А57-10155/2010 №А57-22498/2008 связаны между собой по основаниям возникновения, а при разрешении спора надлежит установить наличие/отсутствие права собственности на земельный участок под объектами построенными ОАО «Совхоз-Весна» и, как следствие, законность создания данных сооружений, судебная коллегия приходит к выводу о необходимости объединить указанные дела в одно производство для совместного рассмотрения с точки зрения более правильного разрешения спора между сторонами.'
 }
 : = {
  Text = 'Учитывая изложенное, следует признать, что при вынесении обжалуемого определения судом нарушены нормы процессуального права, что в соответствии с положениями статьи 270, 272 Арбитражного процессуального кодекса Российской Федерации является основанием для его отмены и направления вопроса на новое рассмотрение в суд первой инстанции.'
 }
 : = {
  Text = 'В соответствии с частью 7 статьи 130 АПК РФ определение арбитражного суда об отказе в удовлетворении ходатайства об объединении дел в одно производство, о выделении требований в отдельное производство может быть обжаловано лицом, подавшим соответствующее ходатайство, в срок, не превышающий десяти дней со дня вынесения данного определения, в арбитражный суд апелляционной инстанции. '
 }
 : = {
  Text = 'Руководствуясь статьями 269-272 Арбитражного процессуального кодекса Российской Федерации, арбитражный суд '
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'ПОСТАНОВИЛ:'
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'Определение Арбитражного суда Саратовской области от 22 сентября 2011 года по делу №  А57-10155/2010 отменить.'
 }
 : = {
  Text = 'Направить вопрос на новое рассмотрение в Арбитражный суд Саратовской области.'
 }
 : = {
  Text = 'Постановление является окончательным, обжалованию в кассационном порядке не подлежит.'
 }
