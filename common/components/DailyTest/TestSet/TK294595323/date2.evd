 : = {
  Text = 'ПОСТАНОВЛЕНИЕ'
 }
 : = {
  Text = 'г. Москва                                                           Дело  № А40-88560/10-51-758'
 }
 : = {
  Text = '31.10.2011'
 }
 : = {
  Text = 'резолютивная часть постановления объявлена 26.10.2011'
 }
 : = {
  Text = 'в полном объеме постановление изготовлено 31.10.2011'
 }
 : = {
  Text = 'Федеральный арбитражный суд Московского округа'
 }
 : = {
  Text = 'в составе:'
 }
 : = {
  Text = 'председательствующего-судьи Петровой Е.А.,'
 }
 : = {
  Text = 'судей Бусаровой Л.В. и Комоловой М.В.,'
 }
 : = {
  Text = 'при участии в заседании:'
 }
 : = {
  Text = 'от истца - Щепина Е.С. по дов. от 19.08.2011, Мельникова Е.С. по дов. от 30.03.2011;  '
 }
 : = {
  Text = 'от ответчика - Кулькова О.Н. по дов. № 416 от 27.05.2011;'
 }
 : = {
  Text = 'от третьих лиц: от ООО «СОХО МЕДИА» - Куменкова О.А. по дов. №30/1 от 17.10.2011, от Рябцева Р.Н. - не явился, извещен;   '
 }
 : = {
  Text = 'рассмотрев в судебном заседании 26.10.2011 кассационную жалобу '
 }
 : = {
  Text = ' ООО «Издательство «ДЖЕМ»'
 }
 : = {
  Text = 'на решение от 12.04.2011 Арбитражного суда города Москвы, принятое судьёй Васильевой Т.В., '
 }
 : = {
  Text = 'на постановление от 11.07.2011 Девятого арбитражного апелляционного суда, принятое судьями Валиевым В.Р., Левченко Н.И., Расторгуевым Е.Б.,'
 }
 : = {
  Text = 'по иску ООО «Издательство «ДЖЕМ» (ИНН 7703037470)'
 }
 : = {
  Text = 'к ЗАО «Сеть телевизионных станций»'
 }
 : = {
  Text = 'о взыскании компенсации за нарушение исключительных прав'
 }
 : = {
  Text = 'с участием третьих лиц: Рябцева Романа Николаевича, ООО «СОХО МЕДИА»'
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'УСТАНОВИЛ: '
 }
 : = {
  Text = ''
 }
 : = {
  Text = 'ООО «Издательство «ДЖЕМ» обратилось в Арбитражный суд г. Москвы с иском к ЗАО «Сеть телевизионных станций» (далее - ЗАО «СТС») о взыскании 180 000 рублей компенсации за нарушение исключительных авторских прав, включая 45 000 рублей компенсации за воспроизведение произведений, 45 000 рублей компенсации за переработку произведений путем включения в состав сложного объекта, 45 000 рублей компенсации за сообщение произведения в эфир, 45 000 рублей  компенсации за сообщение произведения по кабелю.'
 }
 : = {
  Text = 'Исковые требования заявлены на основании ст. ст. 1229, 1250, 1252 Гражданского кодекса РФ (далее - ГК РФ), договора от 28.05.2003 №7 о передаче исключительных авторских прав, заключенного между истцом и Рябцевым Р.Н., и мотивированы тем, что ответчиком в телепередаче «История российского шоу-бизнеса» без разрешения истца как правообладателя использованы тексты песен «Нажми на кнопку», «Полчаса», «Странные танцы».'
 }
 : = {
  Text = 'В качестве третьих лиц, не заявляющих самостоятельных требований относительно предмета спора, к участию в деле привлечены ООО «СОХО МЕДИА», Рябцев Р.Н.'
 }
 : = {
  Text = 'Решением Арбитражного суда города Москвы от 12.04.2011, оставленным без изменения постановлением Девятого арбитражного апелляционного суда от 11.07.2011, в удовлетворении иска отказано.'
 }
 : = {
  Text = '          Решение и постановление мотивированы тем, что ответчик в спорной телепередаче использовал не тексты песен, а иные объекты интеллектуальной деятельности - видеоклипы и концертные выступления, на момент создания которых у истца исключительные права на тексты песен «Нажми на кнопку», «Полчаса», «Странные танцы» отсутствовали. '
 }
 : = {
  Text = '          Суды также указали на то, что правовым основанием использования ответчиком спорных произведений является договор от 5.05.2010 №69/10. '
 }
 : = {
  Text = '           Законность обжалуемых решения и постановления проверена в порядке ст.ст. 284, 286 Арбитражного процессуального кодекса Российской Федерации по кассационной жалобе ООО «Издательство «ДЖЕМ», которое считает, что судами нарушены (неправильно применены) нормы материального права (ст.ст. 1263, 1274 ГК РФ, ст. 13 Закона РФ «Об авторском праве и смежных правах»), сделаны выводы, не соответствующие фактическим обстоятельствам дела и имеющимся в деле доказательствам, не учтено, что в спорной телепередаче ответчиком фрагментарно воспроизводились песни «Нажми на кнопку», «Полчаса», «Странные танцы», исключительные права на которые принадлежат истцу.'
 }
 : = {
  Text = '          В отзывах на кассационную жалобу ответчик и третье лицо - ООО «СОХО МЕДИА» просят оставить обжалуемые судебные акты без изменения, ссылаясь на то, что решение и постановление основаны  на правильном применении норм материального права, судами сделан обоснованный вывод о том, что истцом и ответчиком использованы различные охраняемые объекты интеллектуальной деятельности.'
 }
 : = {
  Text = '          В судебном заседании представители заявителя жалобы поддержали доводы кассационной жалобы, представители ответчика и 3-го лица - ООО «СОХО МЕДИА» возражали против удовлетворения жалобы, полагая, что решение и постановление являются законными и обоснованными, извещенный надлежащим образом Рябцев Р.Н. в судебное заседание не явился, представителя не направил (ч. 3 ст. 284 АПК РФ).'
 }
 : = {
  Text = '   Обсудив доводы кассационной жалобы и отзывов на неё,  выслушав явившихся представителей, проверив в соответствии со ст. 286 Арбитражного процессуального кодекса Российской Федерации правильность применения  судом норм права при вынесении обжалуемых судебных актов, Федеральный арбитражный суд Московского округа приходит к выводу об отсутствии предусмотренных ст. 288 Арбитражного процессуального кодекса Российской Федерации оснований для отмены решения и постановления в связи со следующим.'
 }
 : = {
  Text = 'Предметом оспаривания по настоящему делу является законность использования ответчиком в телепередаче от 23.05.2010 «История российского шоу-бизнеса» произведений «Нажми на кнопку», «Полчаса», «Странные танцы», автором которых является Рябцев Р.Н.'
 }
 : = {
  Text = 'Истец полагал, что обладает исключительными правами на указанные произведения, в связи с чем ответчик без согласия истца не вправе был использовать их в телепередаче.'
 }
 : = {
  Text = 'Отказывая в удовлетворении исковых требований, суды пришли к правильному выводу о том, что ответчиком использованы не сами тексты песен и/или нотная форма, исключительные права на которые переданы истцу по договору от 28.05.2003 №7,  а иные сложные аудиовизуальные произведения: концертные выступления и видеоклипы, созданные в 90-х годах.'
 }
 : = {
  Text = 'Суды правильно исходили из того, что на момент создания использованных ответчиком произведений у истца отсутствовали исключительные права на тексты песен, т.е. 23.05.2010 в телепередаче «История российского шоу-бизнеса»  использованы аудиовизуальные произведения, уже созданные и воспроизведенные до заключения договоров между ООО «СОХО МЕДИА» и Рябцевым Р.Н., а также между Рябцевым Р.Н. и истцом.'
 }
 : = {
  Text = 'Доказательств того, что при создании в 90- годах спорных концертных выступлений и видеоклипов ответчиком были нарушены авторские права на тексты песен «Нажми на кнопку», «Полчаса», «Странные танцы», в материалы дела не представлено, следовательно, суды правильно исходили из того, что ответчиком использованы правомерно созданные аудиовизуальные произведения.'
 }
 : = {
  Text = ' В соответствии с п. 5 ст. 1263 ГК РФ каждый автор произведения, вошедшего составной частью в аудиовизуальное произведение, как существовавшего ранее (автор произведения, положенного в основу сценария, и другие), так и созданного в процессе работы над ним (оператор-постановщик, художник-постановщик и другие), сохраняет исключительное право на свое произведение, за исключением случаев, когда это исключительное право было передано изготовителю или другим лицам либо перешло к изготовителю или другим лицам по иным основаниям, предусмотренным законом.'
 }
 : = {
  Text = 'В данном случае права на использование указанных аудиовизуальных произведений ответчиком в составе телепередачи «История российского шоу-бизнеса» от имени автора песен «Нажми на кнопку», «Полчаса», «Странные танцы» переданы по договору от 5.05.2010 №69/10, который не оспорен и не признан недействительным. '
 }
 : = {
  Text = 'Довод кассационной жалобы о том, что Рябцев Р.Н. физически не мог быть автором аудиовизуальных произведений, использованных ответчиком, а также о том, что подобные произведения создавались телекомпаниями «ВИД», «ЛиС», отклоняется, поскольку отсутствуют доказательства оспаривания и признания договора от 5.05.2010 №69/10 недействительным.'
 }
 : = {
  Text = 'Кроме того, истец не представил доказательств наличия у него полномочий выступать в арбитражном суде в  защиту прав и законных интересов иных лиц.  '
 }
 : = {
  Text = 'Ссылки в кассационной жалобе о том, что помимо аудиовизуальных произведений ответчиком фрагментарно использованы музыкальные произведения также отклоняются, поскольку согласно исковому заявлению предметом оспаривания являлись сложные аудиовизуальные произведения, использованные ответчиком.'
 }
 : = {
  Text = 'При таких обстоятельствах суд кассационной инстанции приходит к выводу о том, что оснований, предусмотренных ст. 288 Арбитражного процессуального кодекса Российской Федерации, для отмены обжалуемых решения и постановления не имеется.     '
 }
 : = {
  Text = 'Руководствуясь ст. ст. 287,  288, 289 Арбитражного процессуального кодекса Российской Федерации, Федеральный арбитражный суд Московского округа       '
 }
 : = {
  Text = 'ПОСТАНОВИЛ:'
 }
 : = {
  Text = 'Решение Арбитражного суда города Москвы от 12.04.201 и постановление Девятого арбитражного апелляционного суда от 11.07.2011 по делу №А40-88560/10-51-758 оставить без изменения, а кассационную жалобу - без удовлетворения.'
 }
