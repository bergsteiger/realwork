  %SUMMARY %SUM
   'Определяет тот факт, что слово aWord уже использовалось для генерации'
  ; // %SUMMARY %SUM
  
  TtfwWord IN aWord
  ARRAY STATIC-VAR g_WasGenerated
  if
   (
    aWord
    g_WasGenerated
    array:Has
    !
   )
  then
   BEGIN
    aWord
    >>>[]
    g_WasGenerated
    false
    Result
    pop:Word:SetValue
   END
  else
   BEGIN
    true
    Result
    pop:Word:SetValue
   END
