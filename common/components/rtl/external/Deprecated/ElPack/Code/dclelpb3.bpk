# ---------------------------------------------------------------------------
!if !$d(BCB)
BCB = $(MAKEDIR)\..
!endif

# ---------------------------------------------------------------------------
# IDE SECTION
# ---------------------------------------------------------------------------
# The following section of the project makefile is managed by the BCB IDE.
# It is recommended to use the IDE to change any of the values in this
# section.
# ---------------------------------------------------------------------------

VERSION = BCB.03
# ---------------------------------------------------------------------------
PROJECT = dclelpB3.bpl
OBJFILES = Design\MlCapProp.obj Design\ElTBDsgn.obj Design\FormCtlProp.obj \
  Design\frmFormPers.obj Design\frmItemCol.obj Design\frmItemsProp.obj \
  Design\frmSectEdit.obj Design\frmSectProp.obj Design\frmSoundMap.obj \
  Design\frmStrPoolEdit.obj Design\PgCtlProp.obj \
  Design\ElMenuDsgn.obj Design\ElEBDsgn.obj Design\TreeDsgn.obj \
  Design\frmStyleImageSelect.obj Design\ElStyleManProp.obj \
  ElReg.obj dclelpB3.obj
RESFILES = dclelpB3.res
DEFFILE =
RESDEPEN = $(RESFILES) Design\MlCapProp.dfm Design\frmFormPers.dfm Design\frmItemCol.dfm \
  Design\frmItemsProp.dfm Design\frmSectEdit.dfm Design\frmSectProp.dfm \
  Design\frmStyleImageSelect.dfm Design\ElStyleManProp.dfm \
  Design\frmSoundMap.dfm Design\frmStrPoolEdit.dfm Design\ElMenuDsgn.dfm

LIBFILES =
LIBRARIES =
SPARELIBS = VCL35.lib vclx35.lib
PACKAGES = vclx35.bpi VCL35.bpi dclstd35.bpi elpackb3.bpi
# ---------------------------------------------------------------------------
PATHCPP = .;
PATHASM = .;Design
PATHPAS = .;Design
PATHRC = .;
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
# ---------------------------------------------------------------------------
CFLAG1 = -O2 -Hc -w -Ve -k- -vi -c -b- -w-par -w-inl -Vx
CFLAG2 = -Idesign;$(BCB)\include;$(BCB)\include\vcl -D_RTLDLL;USEPACKAGES \
  -H=$(BCB)\lib\vcl35.csm
CFLAG3 = -Tkh30000 -4
PFLAGS = -U$(BCB)\lib\release;source;design;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -Isource;design;$(BCB)\include;$(BCB)\include\vcl -D_RTLDLL;USEPACKAGES;VER110 -$L- -$I- -$D- \
  -$C- -v -JPHN -M
RFLAGS = -idesign;$(BCB)\include;$(BCB)\include\vcl -D_RTLDLL;USEPACKAGES
AFLAGS = /i$(BCB)\include /i$(BCB)\include\vcl /d_RTLDLL /dUSEPACKAGES /mx /w2 /zn
LFLAGS = -L$(BCB)\lib\release;design;source;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -D"EldoS Visual Components (Design-Time)" -aa -Tpp -Gpd -x -Gn -Gl -Gi
IFLAGS =
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj $(PACKAGES) sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
# ---------------------------------------------------------------------------
!ifdef IDEOPTIONS

[Version Info]
IncludeVerInfo=1
AutoIncBuild=1
MajorVer=3
MinorVer=0
Release=0
Build=0
