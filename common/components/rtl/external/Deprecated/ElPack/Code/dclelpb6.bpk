VERSION = BCB.06.00
# ---------------------------------------------------------------------------
PROJECT = dclelpB6.bpl
OBJFILES = Design\ElTBDsgn.obj Design\frmFormPers.obj Design\frmItemCol.obj \
    Design\frmItemsProp.obj Design\frmSectEdit.obj Design\PgCtlProp.obj \
    Design\frmSectProp.obj Design\frmSoundMap.obj Design\frmStrPoolEdit.obj \
    Design\ElMenuDsgn.obj Design\ElEBDsgn.obj \
    Design\frmStyleImageSelect.obj Design\ElStyleManProp.obj \
    Design\ElImageIndexProp.obj ElReg.obj dclelpB6.obj
RESFILES = dclelpB6.res
MAINSOURCE = dclelpB6.cpp
RESDEPEN = $(RESFILES) Design\frmFormPers.dfm Design\frmItemCol.dfm \
    Design\frmItemsProp.dfm Design\frmSectEdit.dfm Design\frmSectProp.dfm \
    Design\frmStyleImageSelect.dfm Design\ElStyleManProp.dfm \
    Design\frmSoundMap.dfm Design\frmStrPoolEdit.dfm Design\ElMenuDsgn.dfm
LIBFILES = 
IDLFILES = 
IDLGENFILES = 
LIBRARIES = 
SPARELIBS = VCL.lib designide.bpi
PACKAGES = rtl.bpi vclx.bpi VCL.bpi elpackB6.bpi dclstd.bpi designide.bpi
DEFFILE = 
OTHERFILES = 
# ---------------------------------------------------------------------------
DEBUGLIBPATH = $(BCB)\lib\debug
RELEASELIBPATH = $(BCB)\lib\release
USERDEFINES = 
SYSDEFINES = NO_STRICT;_RTLDLL;_VIS_NOLIB;USEPACKAGES
INCLUDEPATH = design;source;$(BCB)\include;$(BCB)\include\vcl
LIBPATH = $(BCB)\lib\release;design;source;$(BCB)\lib\obj;$(BCB)\lib
WARNINGS= -w-par -w-8027 -w-8026
PATHCPP = .;
PATHASM = .;
PATHPAS = .;Design;Source
PATHRC = .;
PATHOBJ = .;$(LIBPATH)
# ---------------------------------------------------------------------------
CFLAG1 = -O2 -Hc -Vx -Ve -Tkh30000 -X- -a8 -4 -b- -k- -vi -c -tWM
IDLCFLAGS = -src_suffix cpp -Idesign -Isource -I$(BCB)\include -I$(BCB)\include\vcl -boa
PFLAGS = -U$(BCB)\lib\release;source;design;$(BCB)\lib\obj;$(BCB)\lib;$(RELEASELIBPATH) \
  -Idesign;$(BCB)\include;$(BCB)\include\vcl -$YD -$L- -$I- \
  -$D- -$C- -v -M -JPHNE -M -LUdesignide -LUvcl
RFLAGS = 
AFLAGS = /mx /w2 /zn
LFLAGS = -D"EldoS Visual Components (Design-Time)" -aa -Tpp -Gpd -x -Gn -Gl -Gi
# ---------------------------------------------------------------------------
ALLOBJ = c0pkg32.obj $(PACKAGES) Memmgr.Lib sysinit.obj $(OBJFILES)
ALLRES = $(RESFILES)
ALLLIB = $(LIBFILES) $(LIBRARIES) import32.lib cp32mti.lib
