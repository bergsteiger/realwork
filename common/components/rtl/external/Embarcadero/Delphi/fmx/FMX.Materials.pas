  public
    constructor Create; override;
    destructor Destroy; override;
    property Diffuse: TAlphaColor read FDiffuse write FDiffuse;
    property Ambient: TAlphaColor read FAmbient write FAmbient;
    property Emissive: TAlphaColor read FEmissive write FEmissive;
    property Specular: TAlphaColor read FSpecular write FSpecular;
    property Texture: TTexture read FTexture write FTexture;
    property Shininess: Integer read FShininess write FShininess;
  end;

implementation

{ TCustomMaterial }

procedure TCustomMaterial.DoApply(const Context: TContext3D);
begin
  Context.SetShaders(FVertexShader, FPixelShader);
end;

class function TCustomMaterial.DoGetMaterialProperty(const Prop: TMaterial.TProperty): string;
begin
  case Prop of
    TProperty.ModelViewProjection: Result := 'MVPMatrix';
  else
    Result := '';
  end;
end;

