{ TLightMaterial }

  TLightMaterial = class(TCustomMaterial)
  private
    FDiffuse: TAlphaColor;
    FShininess: Integer;
    FAmbient: TAlphaColor;
    FSpecular: TAlphaColor;
    FEmissive: TAlphaColor;
