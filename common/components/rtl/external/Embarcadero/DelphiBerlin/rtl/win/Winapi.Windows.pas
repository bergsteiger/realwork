function FILEMUIINFO_GET_MAIN_TYPEID(pInfo: PFileMUIInfo; iType: DWORD): DWORD;
begin
  if (iType < pInfo.dwTypeIDMainSize) and (pInfo.dwTypeIDMainOffset > 0) then
    Result := PDWORD(NativeUInt(pInfo) + (pInfo.dwTypeIDMainOffset) + iType * sizeof(DWORD))^
