// #define MAKELANGID(p, s)       ((((WORD  )(s)) << 10) | (WORD  )(p))
function MAKELANGID(p, s: WORD): WORD;
begin
  Result := WORD(s shl 10) or p;
end;

// #define PRIMARYLANGID(lgid)    ((WORD  )(lgid) & 0x3ff)
function PRIMARYLANGID(lgid: WORD): WORD;
begin
  Result := WORD(lgid and $3FF);
end;

// #define SUBLANGID(lgid)        ((WORD  )(lgid) >> 10)
function SUBLANGID(lgid: WORD): WORD;
begin
  Result := lgid shr 10;
end;

//#define MAKELCID(lgid, srtid)  ((DWORD)((((DWORD)((WORD  )(srtid))) << 16) |  \
//                                         ((DWORD)((WORD  )(lgid)))))
function MAKELCID(lgid, srtid: WORD): DWORD;
begin
  Result := (srtid shl 16) or lgid;
end;

//#define MAKESORTLCID(lgid, srtid, ver)                                            \
//                               ((DWORD)((MAKELCID(lgid, srtid)) |             \
//                                    (((DWORD)((WORD  )(ver))) << 20)))
function MAKESORTLCID(lgid, srtid, ver: WORD): DWORD;
begin
  Result := MAKELCID(lgid, srtid) or (DWORD(ver) shl 20);
end;

//#define LANGIDFROMLCID(lcid)   ((WORD  )(lcid))
function LANGIDFROMLCID(lcid: DWORD): WORD;
begin
  Result := WORD(lcid and $FFFF);
end;

//#define SORTIDFROMLCID(lcid)   ((WORD  )((((DWORD)(lcid)) >> 16) & 0xf))
function SORTIDFROMLCID(lcid: DWORD): WORD;
begin
  Result := WORD((lcid shr 16) and $F);
end;

//#define SORTVERSIONFROMLCID(lcid)  ((WORD  )((((DWORD)(lcid)) >> 20) & 0xf))
function SORTVERSIONFROMLCID(lcid: DWORD): WORD;
begin
  Result := WORD((lcid shr 20) and $F);
end;


