function IS_HIGH_SURROGATE(wch: Word) : Boolean;
begin
  Result := (wch >= HIGH_SURROGATE_START) and (wch <= HIGH_SURROGATE_END);
end;

function IS_LOW_SURROGATE(wch: Word): Boolean;
begin
  Result := (wch >= LOW_SURROGATE_START) and (wch <= LOW_SURROGATE_END);
end;

function IS_SURROGATE_PAIR(hs, ls: Word): Boolean;
begin
  Result := IS_HIGH_SURROGATE(hs) and IS_LOW_SURROGATE(ls);
end;

function FILEMUIINFO_GET_CULTURE(pInfo: PFileMUIInfo): LPWSTR;
begin
  if pInfo.dwLanguageNameOffset > 0 then
    Result := LPWSTR(NativeUInt(pInfo) + (pInfo.dwLanguageNameOffset))
  else
    Result := nil;
end;

