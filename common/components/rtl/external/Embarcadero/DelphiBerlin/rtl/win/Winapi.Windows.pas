function GID_ROTATE_ANGLE_TO_ARGUMENT(Arg: Double): USHORT;
begin
  Result := Round(((Arg + 2.0 * 3.14159265) / (4.0 * 3.14159265)) * 65535.0);
end;

function GID_ROTATE_ANGLE_FROM_ARGUMENT(Arg: USHORT): Double;
begin
  Result := (((Arg / 65535.0) * 4.0 * 3.14159265) - 2.0 * 3.14159265);
end;

function RotateAngleFromArgument(Arg: Word): Double;
begin
  Result := GID_ROTATE_ANGLE_FROM_ARGUMENT(Arg);
end;

function InertiaVectorFromArgument(Arg: UInt64): TSmallPoint;
begin
  Result.x := SmallInt(Arg shr 32);
  Result.y := SmallInt(HiWord(Arg shr 32));
end;

procedure InitVersionInfo;
var
  OSVersionInfo: TOSVersionInfo;
begin
  OSVersionInfo.dwOSVersionInfoSize := SizeOf(OSVersionInfo);
  if GetVersionEx(OSVersionInfo) then
    with OSVersionInfo do
    begin
      Win32Platform := dwPlatformId;
      Win32MajorVersion := dwMajorVersion;
      Win32MinorVersion := dwMinorVersion;
    end;
end;

