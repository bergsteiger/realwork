{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $48000000}
{$DESCRIPTION 'JEDI Code Library VisualCLX package for Delphi 6'}
{$LIBSUFFIX '60'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  rtl,
  visualclx,
  DJcl;

contains
  JclQGraphUtils in '..\..\source\visclx\JclQGraphUtils.pas' ,
  JclQGraphics in '..\..\source\visclx\JclQGraphics.pas' ;

end.
