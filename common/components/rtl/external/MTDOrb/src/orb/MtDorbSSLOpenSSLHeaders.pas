function LoadFunction(FceName:String):Pointer;
begin
  FceName := FceName+#0;
  {$ifdef fpc}
  Result := GetProcAddress(hIdSSL, FceName);
  {$else}
  Result := GetProcAddress(hIdSSL, PAnsiChar(@FceName[1]));
  {$endif}
//  if (Result = nil) then ShowMessage('Error loading: ' + FceName);
end;

