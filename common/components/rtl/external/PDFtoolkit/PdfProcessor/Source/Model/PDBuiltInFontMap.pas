type

  TgtBuiltInFont = class(TgtObject)
  private
    FName: AnsiString;
    FAscent: Double;
    FDescent: Double;
    FBBox: TgtPDRect;
    FSize: Integer;
    FWidths: array of TgtBuiltInFontWidths;
    FEncodingName: AnsiString;

    constructor Create(const FontName: AnsiString);
  public
    FDefaultBaseEnc: TgtEncodingArray;
    destructor Destroy; override;
    class function MakeBuiltInFont(const FontName: AnsiString): TgtBuiltInFont;
    property Name: AnsiString read FName;
    property Ascent: Double read FAscent;
    property Descent: Double read FDescent;
    property BBox: TgtPDRect read FBBox;
    property EncodingName: AnsiString read FEncodingName;
    //function DefaultBaseEnc: TgtEncodingArray;
    function GetWidth(const AChar: AnsiString): Double; overload;
  end;

implementation

uses
  PDTypes;

{ TgtBuiltInFont }

constructor TgtBuiltInFont.Create(const FontName: AnsiString);
begin
  FName := FontName;
end;
(*
function TgtBuiltInFont.DefaultBaseEnc: TgtEncodingArray;
begin
  Result := FDefaultBaseEnc;
end;
*)
destructor TgtBuiltInFont.Destroy;
begin
  FBBox.Free;
  SetLength(FWidths, 0);
  inherited;
end;

function TgtBuiltInFont.GetWidth(const AChar: AnsiString): Double;
var
  First, Last, Mid, CmpResult: Integer;
begin
  CmpResult := 0;
  Result := -1.0;
  First := Low(FWidths);
  Last := High(FWidths);
  if AChar <> '' then
  begin
    while (First <= Last) do
    begin
      Mid := (First + Last) div 2;
      CmpResult := AnsiCompareStr(FWidths[Mid].Character, AChar);
      if CmpResult < 0 then
        First := Mid + 1
      else if CmpResult > 0 then
        Last := Mid - 1
      else
      begin
        First := Mid;
//        Last  := Mid;
        Break;
      end;
    end;

    if CmpResult = 0 then
    begin
      Result := FWidths[First].Width;
    end;
  end;
end;

