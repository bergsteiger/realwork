package X250SR50;

{$R *.RES}
{$ALIGN ON}
{$ASSERTIONS ON}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS ON}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION OFF}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO ON}
{$SAFEDIVIDE OFF}
{$STACKFRAMES ON}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST ON}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'XMLPartner 2.5 Runtime package - VCL50'}
{$RUNONLY}
{$IMPLICITBUILD ON}

requires
  vcl50;

contains
  XpBase in 'xpbase.pas',
  xpchrflt in 'xpchrflt.pas',
  XpDom in 'XpDOM.pas',
  XpExcept in 'xpexcept.pas',
  XpParser in 'XpParser.pas',
  xpAbout in 'xpAbout.pas' {XpAboutBox},
  xpinet in 'xpinet.pas';

end.
