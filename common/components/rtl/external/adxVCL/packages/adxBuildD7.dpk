package adxBuildD7;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{$DEBUGINFO OFF}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Add-in Express 2010 for VCL run-time package for Delphi 7'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  rtl,
  vcl,
  dclOffice2k;

contains
  adxAddIn in '..\adxAddIn.pas',
  adxAddInDesignerObjects in '..\adxAddInDesignerObjects.pas',
  adxOffice in '..\adxOffice.pas',
  adxRTDServ in '..\adxRTDServ.pas',
  FrontPage2000 in '..\FrontPage2000.pas',
  FrontPageEditor2000 in '..\FrontPageEditor2000.pas',
  Project2000 in '..\Project2000.pas',
  MapPoint2002 in '..\MapPoint2002.pas',
  Visio2002 in '..\Visio2002.pas',
  adxSmartTag in '..\adxSmartTag.pas',
  adxSmartTagTLB in '..\adxSmartTagTLB.pas',
  Publisher2003 in '..\Publisher2003.pas',
  adxMSFormsControls in '..\adxMSFormsControls.pas',
  adxHostAppEvents in '..\adxHostAppEvents.pas',
  InfoPath2007 in '..\InfoPath2007.pas';

end.
