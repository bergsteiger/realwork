package adxolBuildD7;

{$R *.res}
{$ALIGN 8}
{$ASSERTIONS OFF}
{$BOOLEVAL OFF}
{$DEBUGINFO ON}
{$EXTENDEDSYNTAX ON}
{$IMPORTEDDATA ON}
{$IOCHECKS ON}
{$LOCALSYMBOLS OFF}
{$LONGSTRINGS ON}
{$OPENSTRINGS ON}
{$OPTIMIZATION ON}
{$OVERFLOWCHECKS OFF}
{$RANGECHECKS OFF}
{$REFERENCEINFO OFF}
{$SAFEDIVIDE OFF}
{$STACKFRAMES OFF}
{$TYPEDADDRESS OFF}
{$VARSTRINGCHECKS ON}
{$WRITEABLECONST OFF}
{$MINENUMSIZE 1}
{$IMAGEBASE $400000}
{$DESCRIPTION 'Add-in Express 2010 Extensions VCL run-time package for Delphi 7'}
{$RUNONLY}
{$IMPLICITBUILD OFF}

requires
  rtl,
  adxBuildD7,
  vclx,
  dbrtl,
  dclOffice2k,
  vcl,
  vcldb;

contains
  adxolFormsManager in '..\adxolFormsManager.pas',
  adxWSbcls in '..\adxWSbcls.pas',
  adxxlFormsManager in '..\adxxlFormsManager.pas',
  adxwdFormsManager in '..\adxwdFormsManager.pas',
  adxppFormsManager in '..\adxppFormsManager.pas',
  adxGdiPAPI in '..\adxGdiPAPI.pas',
  adxCore in '..\adxCore.pas';

end.
