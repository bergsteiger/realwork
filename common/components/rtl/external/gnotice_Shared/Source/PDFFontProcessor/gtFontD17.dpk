  rtl,
  vcl,
  gtRtlD17;

contains
  gtPDFFont3 in 'gtPDFFont3.pas',
  gtGlyphInfo in 'gtGlyphInfo.pas',
  gtDocUsp10 in 'gtDocUsp10.pas',
  gtMLang in 'gtMLang.pas';

