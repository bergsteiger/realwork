{$If not defined(Admin) AND not defined(Monitorings)}
function TdmStdRes.pm_GetSaveDialog: TnsSaveDialog;
begin
 if (f_SaveDialog = nil) then
 begin
  f_SaveDialog := TnsSaveDialog.Create(self);
  f_SaveDialog.Title := vcmConstString(str_SaveAsDialogTitle);
  f_SaveDialog.DefaultExt := vcmConstString(str_RTFFileExtension);
  f_SaveDialog.Filter := vcmConstString(str_srSaveDialogFilter);
  f_SaveDialog.Filter := f_SaveDialog.Filter + vcmConstString(str_srSaveDialogFilterXMLAppend);
 {$IfOpt D+}
  f_SaveDialog.Filter := f_SaveDialog.Filter + vcmConstString(str_srSaveDialogFilterEvdAppend); // for debug only
  f_SaveDialog.Filter := f_SaveDialog.Filter + vcmConstString(str_srSaveDialogFilterNullAppend); // for debug only
 {$endif}
  f_SaveDialog.FilterIndex := 1;
  // форма сохранения в файл до Windows 98 не поддерживает ресайз
  if GetWindowsVersion in [wvWin95, wvWin95OSR2, wvWinNT31, wvWinNT35, wvWinNT351, wvWinNT4] then
   f_SaveDialog.Options := [ofOverwritePrompt, ofHideReadOnly, ofPathMustExist]
  else
   f_SaveDialog.Options := [ofOverwritePrompt, ofHideReadOnly, ofEnableSizing, ofPathMustExist];
 end;//f_SaveDialog = nil
 Result := f_SaveDialog;
end;//pm_GetSaveDialog
{$IfEnd}

