procedure TdmStdRes.MenuManagerInitCommands(aSender, aCommands: TObject);
begin
 with (aCommands As TOvcCommandProcessor) do
 begin
  try
   // - добавляем ShortCut'ы которые никогда не приходят контролам
(*   AddCommand(StopExtTableName, VK_LEFT,  [ssCtrl]);
   AddCommand(StopExtTableName, VK_RIGHT, [ssCtrl]);*)
   // - убрано, т.к. это сейчас давится на уровне редактора
   AddCommand(StopExtTableName, VK_UP,       [ssCtrl]);
   AddCommand(StopExtTableName, VK_DOWN,     [ssCtrl]);
   (* AddCommand(StopExtTableName, VK_RETURN); *)
   AddCommand(StopExtTableName, VK_RETURN,   [ssCtrl]);
   AddCommand(StopExtTableName, VK_DELETE,   [ssCtrl]);
  except
   on E: Exception do
    l3System.Exception2Log(E);
  end;//try..except
  {убиваем "лишние" shortcut'ы}
  try
   DeleteCommand(OvcCmd.scListTableName, Ord('A'), OvcByteShift([ssCtrl]));
   DeleteCommand(OvcCmd.scListTableName, VK_INSERT);
   DeleteCommand(OvcCmd.scListTableName, VK_DELETE);
  except
   on E: Exception do
    l3System.Exception2Log(E);
  end;//try..except
 end;//with aCommands
end;

procedure TdmStdRes.MenuManagerGlyphSizeChanged(Sender: TObject);

 function lp_CalcSize: TvtPILSize;
 var
  l_Width: Integer;
 begin
  l_Width := Screen.Monitors[0].Width;
  if l_Width <= 800 then
   Result := ps16x16
  else
   if l_Width <= 1024 then
    Result := ps24x24
   else
    Result := ps32x32
 end;

begin
 case MenuManager.GlyphSize of
  vcm_gsAutomatic:
   ImageList.CurSize := lp_CalcSize;
  vcm_gs16x16:
   ImageList.CurSize := ps16x16;
  vcm_gs24x24:
   ImageList.CurSize := ps24x24;
  vcm_gs32x32:
   ImageList.CurSize := ps32x32;
 end;
end;

procedure TdmStdRes.MenuManagerGlyphColordepthChanged(Sender: TObject);
var
 l_ColorDepth: TvtPILBpp;
begin
 l_ColorDepth := AutoDetectColorDepth;
 case MenuManager.GlyphColordepth of
  vcm_gc16        : l_ColorDepth := bpp4;
  vcm_gc256       : l_ColorDepth := bpp8;
  vcm_gcTrueColor : l_ColorDepth := bpp24;
 end;
 SetColorDepthGlobal(l_ColorDepth);
end;

