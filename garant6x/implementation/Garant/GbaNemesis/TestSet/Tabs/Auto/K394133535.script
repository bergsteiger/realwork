 PROCEDURE "Выбрать ноду номер" INTEGER IN aNum
  aNum раз ( "Стрелка вниз" )
  "Нажать Enter"
 ;

 Параметры: ( "Открывать документы из списка в текущем окне"  )
 Выполнить (
  INTEGER VAR l_NodeCount
  INTEGER VAR item
  item := 0

  ППР
  "Перевести фокус в поле 'Вид правовой информации' в ППР"
  @ ( "Нажать Enter" ) MODAL ( l_NodeCount := ( контрол::AttributeTree:push tree:GetItemsCount ) )
  l_NodeCount раз (
   ППР
   "Перевести фокус в поле 'Вид правовой информации' в ППР"
   @ ( "Нажать Enter" ) MODAL ( "Выбрать ноду номер {(item)}" )
   "Нажать Искать"
   "Перевести фокус в список"
   "Нажать Enter"
   5 раз ( 
    "Дать системе перерисоваться"
    "Следующий документ в списке" 
   )
   "Дать системе перерисоваться"
   ОМ
   "Очистить журнал работы"
   item := ( item ++ )
  )
 )
;

