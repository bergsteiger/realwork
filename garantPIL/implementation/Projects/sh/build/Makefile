help:
	@echo "Available targets:"
	@echo "ace tao garant_tools garant_server mlma32ru"
	@echo "ace_release tao_release garant_tools_release garant_server_release mlma32ru_release"
	@echo "ace_static garant_tools_static garant_server_static *)"
	@echo "ace_release_static garant_tools_release_static garant_server_release_static *)"
	@echo "*) static build is preferable and recommended"
	@echo "Available clean targets:"
	@echo "clean_<Available_target_name>, where <Available_target_name> = set of Available targets listed above"
	@echo "update_cvs"

.PHONY: help

all:	help
