[ORB Params]
-ORBDottedDecimalAddresses=1
-ORBUseSharedProfiles=1
;-ORBDebugLevel=1

[ServerManager]
-EMailTo=$SMTP_MAIL_TO$
-InfoCommand=##INSTALL_PATH##\apps\winNT\F1ServerInfo.exe
-InfoCommandLine=-GCMConfigFile "##INSTALL_PATH##\garant.ini" -GPMF1ServerInfoConfigSection "F1ServerInfo Params"
-RefreshTimeout=30
-Restart=
-ResultFile=##INSTALL_PATH##\apps\winNT\F1ServerInfo.results
-SMTPServer=
-SMTPServerLogin=
-SMTPServerPassword=
-SMTPServerPort=25
-Services=GarantF1Server
;-AutoRestart=

[Version]
-IniFile=3
-Product=server
