          Result := MainW.VCLObject('tsOperation').VCLObject('GroupBox6').VCLObject('clb_Consulting'); 
        // НАМНБКЕМХЪ Х ГЮЙЮГШ
        //// НАМНБКЕМХЪ
        'намнбкемхъ х гюйюгш/яоняна намнбкемхъ':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('cb_UpdateType');  
        'намнбкемхъ х гюйюгш/вюярнрю намнбкемхъ':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('cb_UpdateInterval');
        'намнбкемхъ х гюйюгш/яоняна днярюбйх':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('clb_TransType');                                 
        'намнбкемхъ х гюйюгш/гю яв╗р по нр':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('ce_UpdateSelfDiscountFrom');
        'намнбкемхъ х гюйюгш/гю яв╗р по дн':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('ce_UpdateSelfDiscountTo'); 
        'намнбкемхъ х гюйюгш/нанямнбюмхе яйхдйх по':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('cb_UpdSelfDisType');
        'намнбкемхъ х гюйюгш/гю яв╗р моо+по нр':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('ce_UpdateDiscountFrom');                                 
        'намнбкемхъ х гюйюгш/гю яв╗р моо+по дн':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('ce_UpdateDiscountTo');
        'намнбкемхъ х гюйюгш/нанямнбюмхе яйхдйх моо+по':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox1').VCLObject('cb_UpdDisType');
        //// ОЮПЮЛЕРПШ ДХЯРПХАСРХБЮ
        'намнбкемхъ х гюйюгш/рхо дхярпхасрхбю':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox4').VCLObject('cb_DistrType');  
        'намнбкемхъ х гюйюгш/рхо яепбепю':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox4').VCLObject('cb_ServType');
        'намнбкемхъ х гюйюгш/яоняна гюыхрш':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox4').VCLObject('clb_SecType');                                 
        'намнбкемхъ х гюйюгш/рхо хмярюккъжхх':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox4').VCLObject('cb_InstType');
        'намнбкемхъ х гюйюгш/мняхрекэ':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox4').VCLObject('clb_Media');
        //// ХМДХБХДСЮКЭМШЕ ОНДАНПЙХ 
        'намнбкемхъ х гюйюгш/щкейрпнммюъ онврю', 'намнбкемхъ х гюйюгш/E-MAIL':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox3').VCLObject('edPersSerEMail'); 
        'намнбкемхъ х гюйюгш/пюяяшкюрэ он E-MAIL':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox3').VCLObject('chkPersSerActive');
        'намнбкемхъ х гюйюгш/бшякюрэ оевюрмсч тнплс':
          Result := MainW.VCLObject('tsUpdates').VCLObject('GroupBox3').VCLObject('chkSendPrintForm');
        // ЙМНОЙХ
        'нвхярхрэ':
          Result := Btns.VCLObject('btnClean'); 
        'бепмсрэяъ':
          Result := Btns.VCLObject('btnRestore');
        'хяйюрэ':
          Result := Btns.VCLObject('btnSearch');
        'нрлемю':
          Result := Btns.VCLObject('BitBtn2');
        
      else 
        Raise('МЕ ДНОСЯРХЛШИ ОЮПЮЛЕРП');    
      end;
      
    if not IsExists(Result) then 
      Raise('нАЗЕЙРЮ "' + whatToGet + '" МЕ ЯСЫЕЯРБСЕР');            
  except
    Result := NonExObj; 
    Log.Error(ExceptionMessage);
  end;
end;    



// ДХЮКНЦ ПЮЯЬХПЕММНЦН ОНХЯЙЮ
function GetFromHardChange(
  whatToGet : String
) : OleVariant;
var
  MainW, Btns;
begin
  try
    whatToGet := AnsiUpperCase(whatToGet);                                          
    MainW := Sys.Process('Armadillo').VCLObject('frmChangeKIB');
    if not IsExists(MainW) then Raise('НЙМН "яКНФМШИ ОЕПЕУНД Я йха МЮ йха" МЕ НРЙПШРН');

      case whatToGet of
        '':
          Result := MainW;
        // РЮАКХЖШ 
        'хлечыхеяъ опндсйрш':
          Result := MainW.VCLObject('GroupBox1').VCLObject('hlSourceProducts');
        'оепеирх мю', 'оепеирх мю...':
          Result := MainW.VCLObject('GroupBox2').VCLObject('hlChangedProducts');
          
        // ЙМНОЙХ
        'днаюбхрэ мнбши опндсйр', 'днаюбхрэ':
          Result := MainW.VCLObject('btnAddProduct');
        'педйюрхпнбюрэ оюпюлерпш опндсйрю', 'педюйрхпнбюрэ':
          Result := MainW.VCLObject('btnEditProduct');
        'сдюкхрэ опндсйр', 'сдюкхрэ':
          Result := MainW.VCLObject('btnDelProduct');
          
        //  
        'дюрю ноепюжхх':
          Result := MainW.VCLObject('Panel2').VCLObject('dtDateChange');
        '15':
          Result := MainW.VCLObject('Panel2').VCLObject('dtDateChange').VCLObject('TEditButton');
        'нрберярбеммиши':
          Result := MainW.VCLObject('Panel2').VCLObject('cb_Courier');
        'яйхдйю мю ноепюжхч', 'яйхдйю':
          Result := MainW.VCLObject('pOperationDiscounts').VCLObject('edDiscount');
        'наняммнбюмхе':
          Result := MainW.VCLObject('pOperationDiscounts').VCLObject('cb_DisType');
          
        // ЙМНОЙХ 
        'опхмърэ', 'нй', 'OK':
          Result := MainW.VCLObject('btnOK');
        'нрлемю':
          Result := MainW.VCLObject('BitBtn2');
        
      else 
        Raise('МЕ ДНОСЯРХЛШИ ОЮПЮЛЕРП');    
      end;
      
    if not IsExists(Result) then 
      Raise('нАЗЕЙРЮ "' + whatToGet + '" МЕ ЯСЫЕЯРБСЕР');            
  except
    Result := NonExObj; 
    Log.Error(ExceptionMessage);
  end;
end;    