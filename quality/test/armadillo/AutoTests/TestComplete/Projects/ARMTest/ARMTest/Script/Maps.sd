// ДХЮКНЦ ЯНГДЮМХЪ МНБНЦН ЙКХЕМРЮ
function GetFromAddClient(
  whatToGet : String
) : OleVariant;
var
  MainW;
begin
  try
    whatToGet := AnsiUpperCase(whatToGet);                                          
    MainW := Sys.Process('Armadillo').VCLObject('frmAddClient');
    if not IsExists(MainW) then Raise('НЙМН "бМЕЯЕМХЕ ХМТНПЛЮЖХХ Н МНБНЛ ЙКХЕМРЕ" МЕ НРЙПШРН');

      case whatToGet of
        '' :
          Result := MainW;
        'мюхлемнбюмхе':
          Result := MainW.VCLObject('edShortClientName');
        'опетхйя':
          Result := MainW.VCLObject('cb_Prefix');
        'йюрецнпхъ':
          Result := MainW.VCLObject('cb_UCategory');
        'мюя.осмйр', 'мюя. осмйр':
          Result := MainW.VCLObject('f_CityBox').VCLObject('cb_City');
        'хмдейя':
          Result := MainW.VCLObject('edPostIndex');
        'юдпея':
          Result := MainW.VCLObject('edPostAddress');
        'лемедфеп':
          Result := MainW.VCLObject('cbManager');
        'йспэеп':
          Result := MainW.VCLObject('cb_Courier');  
        'опхмърэ', 'опхлемхрэ', 'нй', 'OK':
          Result := MainW.VCLObject('btnOK');
        'нрлемю', 'CANCEL':
          Result := MainW.VCLObject('btnCancel');                                      
          
          
               
      else 
        Raise('МЕ ДНОСЯРХЛШИ ОЮПЮЛЕРП');    
      end;
      
    if not IsExists(Result) then 
      Raise('нАЗЕЙРЮ "' + whatToGet + '" МЕ ЯСЫЕЯРБСЕР');            
  except
    Result := NonExObj; 
    Log.Error(ExceptionMessage);
  end;
end;    

