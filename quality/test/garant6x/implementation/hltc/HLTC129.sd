  HLTC129.1.3.7: оЕПЕИРХ Б ЯОХЯНЙ ДНЙСЛЕМРНБ, МЮФЮРЭ ЙМНОЙС ═МЮ РСКАЮПЕ <сБЕКХВХРЭ/сЛЕМЭЬХРЭ ЬПХТР> - САЕДХРЯЪ, 
  ВРН Б ЯРЮРСЯМНИ ЯРПНЙЕ МЕ ОНЪБКЪЕРЯЪ РЕЙЯР <гЮЦПСГЙЮ ДНЙСЛЕМРЮ>
}  
function HLTC129_1_3_7_Execute(var  CurrentState: OleVariant): OleVariant;  
var
  toolBar;  
  wnd; 
  statusBar;
  statusText;
begin
try    
  wnd := GetListOfAllDocuments; 
  if NOT IsExists(wnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ ЯОХЯНЙ БЯЕУ ДНЙСЛЕМРНБ!');      
