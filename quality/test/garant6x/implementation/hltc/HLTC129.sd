var
  settings;    
  wnd, treeWnd;
  x, y;
  picture;
  editorWnd;
begin
try
  settings := [
    (['рейяр', 'йнлананйя "мюгбюмхе"', 'Courier']),    
    (['рейяр', 'йнлананйя "жбер"', clRed]),   
