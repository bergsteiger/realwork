Р.Е ОПХ ЕЦН ХГЛЕМЕХХ НМХ НРПЮФЮЧРЯЪ БН БЯЕУ ДЕПЕБЭЪУ:
  HLTC129.1.2.3: б ПСАПХЙЮРНПЕ
}  
function HLTC129_1_2_3_Execute(var  CurrentState: OleVariant): OleVariant;
var
  settings;  
  wnd;
  x, y;
  picture;
begin
try
  settings := [
    (['рейяр', 'йнлананйя "мюгбюмхе"', 'Times New Roman']),    
    (['рейяр', 'йнлананйя "жбер"', clLime]),    
    (['рейяр', 'йнлананйя "пюглеп"', '8']),
    (['рейяр', 'вейанйя "фхпмши"',        true]),
    (['рейяр', 'вейанйя "мюйкнммши"',     true]),
    (['рейяр', 'вейанйя "ондв╗пймсрши"',  true]),
    (['рейяр', 'вейанйя "оепев╗пймсрши"', true])
    
  ];
  if NOT SetSettingsInStyleEditor( settings ) then 
    Raise('мЕ СДЮКНЯЭ СЯРЮМНБХРЭ МЮЯРПНИЙХ Б пЕДЮЙРНПЕ ЯРХКЕИ!');
  
  if NOT OpenObjectFromFolder(LIST_RUBR_MAIN, OpenMenuTab) then 
    Raise('мЕ СДЮКНЯЭ НРЙПШРЭ ПСАПХЙЮРНП!');      
  wnd := GetFromList('яохянй псапхйюрнпю');
  if NOT IsExists(wnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ яохянй псапхйюрнпю');
    
  picture := RenderTextToPicture('оПЮБЮ', 'Times New Roman', 8, 
    'textColor:=clLime; isBold:=true; isItalic:=true; isUnderline:=true; isStrikeout:=true'); 
  if NOT FindPicture(
          wnd, 
          picture,
          x, y
  ) then
  begin
    Log.Picture(picture, 'йЮПРХМЙЮ НРПЕМДЕПЕМНЦН ЬПХТРЮ');
    Log.Picture(wnd, 'йЮПРХМЙЮ НАЗЕЙРЮ, МЮ ЙНРНПНЛ ХЫЕРЯЪ ЬПХТР');
    
    Log.Error('мЕ СДЮКНЯЭ Б ПСАПХЙЮРНПЕ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');
    Raise('мЕ СДЮКНЯЭ Б ПСАПХЙЮРНПЕ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');    
  end;
  
  if NOT SetDefaultSettingsInStyleEditor then 
    Raise('мЕ СДЮКНЯЭ СЯРЮМНБХРЭ МЮЯРПНИЙХ Б пЕДЮЙРНПЕ ЯРХКЕИ ОН СЛНКВЮМХЧ');
    
  result := true;  
except
  SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_2_3_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;

