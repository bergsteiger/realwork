  picture := RenderTextToPicture('дНЙСЛЕМР', 'Courier', 15, 
    'textColor:=clRed; isBold:=true; isItalic:=true; isUnderline:=true; isStrikeout:=true'); 
  if NOT FindPicture(
          treeWnd, 
          picture,
          x, y
  ) then
  begin
    Log.Picture(picture, 'йЮПРХМЙЮ НРПЕМДЕПЕМНЦН ЬПХТРЮ');
    Log.Picture(treeWnd, 'йЮПРХМЙЮ НАЗЕЙРЮ, МЮ ЙНРНПНЛ ХЫЕРЯЪ ЬПХТР');
    
    Log.Error('мЕ СДЮКНЯЭ Б яхрсюжхх днонкмхрекэмнцн спнбмъ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');
