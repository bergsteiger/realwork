  if NOT IsExists(treeWnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ депебн юррпхасрнб');  
    
  picture := RenderTextToPicture('юЙР', 'Courier', 15, 
    'textColor:=clRed; isBold:=true; isItalic:=true; isUnderline:=true; isStrikeout:=true'); 
  if NOT FindPicture(
          treeWnd, 
          picture,
          x, y
  ) then
  begin
    Log.Picture(picture, 'йЮПРХМЙЮ НРПЕМДЕПЕМНЦН ЬПХТРЮ');
    Log.Picture(treeWnd, 'йЮПРХМЙЮ НАЗЕЙРЮ, МЮ ЙНРНПНЛ ХЫЕРЯЪ ЬПХТР');
    
    Log.Error('мЕ СДЮКНЯЭ Б ДЕПЕБЕ ЮРПХАСРНБ ооп МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');
    Raise('мЕ СДЮКНЯЭ Б ДЕПЕБЕ ЮРПХАСРНБ ооп МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');    
  end;
  
  if NOT ClickToolbarButton(GetFromAttributeSelect('мхфмхи рскаюп'), 'нРЛЕМЮ') then   
    Raise('мЕ СДЮКНЯЭ МЮФЮРЭ "нРЛЕМЮ" МЮ МХФМЕЛ ЯОХЯЙЮ ЮРПХАСРНБ!');
  //-----------
  if NOT PositionInQueryCard(wnd, FD_MU_REG_STATUS, [CT_LABEL] ) then
    Raise('мЕ СДЮКНЯЭ Б йг ооп ЯОНГХЖХНМХПНБЮРЭЯЪ МЮ ЙНМРПНК "яРЮРСЯ ПЕЦХЯРПЮЖХХ"'); 
  Sys.Keys('[Enter]');
