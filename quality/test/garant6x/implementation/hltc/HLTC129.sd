{
HLTC129.1.2: оПНБЕПХРЭ, ВРН ДКЪ БЯЕУ ДПЕБНБХДМШУ НАЗЕЙРНБ ХЯОНКЭГСЕРЯЪ ЯРХКЭ <рЕЙЯР>,
Р.Е ОПХ ЕЦН ХГЛЕМЕХХ НМХ НРПЮФЮЧРЯЪ БН БЯЕУ ДЕПЕБЭЪУ:
  HLTC129.1.2.1: дКЪ ЯОХЯЙЮ
}  
function HLTC129_1_2_1_Execute(var  CurrentState: OleVariant): OleVariant;
var
  settings;  
  wnd;
  x, y;
  picture;
begin
try
  settings := [
    (['рейяр', 'йнлананйя "мюгбюмхе"', 'Verdana']),    
    (['рейяр', 'йнлананйя "жбер"', clRed]),
    (['рейяр', 'йнлананйя "тнм"', clWhite]),
    (['рейяр', 'йнлананйя "пюглеп"', '20']),
    (['рейяр', 'вейанйя "фхпмши"',        true]),
    (['рейяр', 'вейанйя "мюйкнммши"',     true]),
    (['рейяр', 'вейанйя "ондв╗пймсрши"',  true]),
    (['рейяр', 'вейанйя "оепев╗пймсрши"', true])
    
  ];
  if NOT SetSettingsInStyleEditor( settings ) then 
    Raise('мЕ СДЮКНЯЭ СЯРЮМНБХРЭ МЮЯРПНИЙХ Б пЕДЮЙРНПЕ ЯРХКЕИ!');
  
  if NOT OpenObjectFromFolder(LIST_MAIN, OpenMenuTab) then 
    Raise('мЕ СДЮКНЯЭ НРЙПШРЭ ЯОХЯНЙ ОН ОЕПБНИ БЕРЙХ!');      
  wnd := GetFromList('яохянй днйслемрнб');
  if NOT IsExists(wnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ яохянй днйслемрнб');
  wnd.SetFocus;
  Sys.Keys('[Down]');
  
  picture := RenderTextToPicture('йНМЯРХРСЖХЪ пНЯЯХИЯЙНИ тЕДЕПЮЖХХ', 'Verdana', 20, 
    'textColor:=clRed; bkColor:=clWhite; isBold:=true; isItalic:=true; isUnderline:=true; isStrikeout:=true'); 
  if NOT FindPicture(
          wnd, 
          picture,
          x, y
  ) then
  begin
    Log.Picture(picture, 'йЮПРХМЙЮ НРПЕМДЕПЕМНЦН ЬПХТРЮ');
    Log.Picture(wnd, 'йЮПРХМЙЮ НАЗЕЙРЮ, МЮ ЙНРНПНЛ ХЫЕРЯЪ ЬПХТР');
    
    Log.Error('мЕ СДЮКНЯЭ Б ЯОХЯЙЕ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');
    Raise('мЕ СДЮКНЯЭ Б ЯОХЯЙЕ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ!');    
  end;
  
  if NOT SetDefaultSettingsInStyleEditor then 
    Raise('мЕ СДЮКНЯЭ СЯРЮМНБХРЭ МЮЯРПНИЙХ Б пЕДЮЙРНПЕ ЯРХКЕИ ОН СЛНКВЮМХЧ');
    
  result := true;  
except
  SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_2_1_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;


