{
  HLTC129.1.13: оПНБЕПХРЭ, ВРН С ЯРХКЪ <рЕЙЯР> МЕКЭГЪ ХГЛЕМРЭ РХО МЮЯКЕДНБЮМХЪ.
}  
function HLTC129_1_13_Execute(var  CurrentState: OleVariant): OleVariant;
var
  editorWnd;
  control;
  controlName;
  i;  
begin
try
  editorWnd := GoToStyleEditor;
  if NOT IsExists(editorWnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ ПЕДЮЙРНП ЯРХКЕИ');
  if NOT Position_Internal(GetFromStyleEditor('депебн ярхкеи'), 'рейяр', '\', true, 'IsPressHome:=true' ) then
    Raise('мЕ СДЮКНЯЭ ЯОНГХЖХНМХПНБЮХРЭЯЪ МЮ ЯРХКЭ: рейяр');
  for i := 1 to 8 do
  begin
    case i of
      1:  controlName := 'ймнойю рхою мюякеднбюмхъ "мюгбюмхе"';
      2:  controlName := 'ймнойю рхою мюякеднбюмхъ "жбер"';
      3:  controlName := 'ймнойю рхою мюякеднбюмхъ "тнм"';      
      4:  controlName := 'ймнойю рхою мюякеднбюмхъ "пюглеп"';
      5:  controlName := 'ймнойю рхою мюякеднбюмхъ "фхпмши"';
      6:  controlName := 'ймнойю рхою мюякеднбюмхъ "мюйкнммши"';
      7:  controlName := 'ймнойю рхою мюякеднбюмхъ "ондв╗пймсрши"';
      8:  controlName := 'ймнойю рхою мюякеднбюмхъ "оепев╗пймсрши"';
    end;//case
    control := GetFromStyleEditor(controlName);
    if NOT IsExists(control) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ ЙНМРПНК: ' + controlName);
    if control.Enabled then
    begin
      Log.Error('щКЕЛЕМР : ' + controlName + ' МЕ ДХГЕИАКЕММШИ, ГМЮВХР ЕЯРЭ БНГЛНФМНЯРЭ ХГЛЕМХРЭ РХО МЮЯКЕДНБЮМХЪ!!');
      Raise('щКЕЛЕМР : ' + controlName + ' МЕ ДХГЕИАКЕММШИ, ГМЮВХР ЕЯРЭ БНГЛНФМНЯРЭ ХГЛЕМХРЭ РХО МЮЯКЕДНБЮМХЪ!!');
    end;
    if control.Caption <> 'я' then
    begin
      Log.Error('рХО МЮЯКЕДНБЮМХЪ ЙНМРПНКЮ : ' + controlName + ' ДКЪ ЯРХКЪ рейяр ОН СЛНКВЮМХЧ МЕ "я"');
      Raise('рХО МЮЯКЕДНБЮМХЪ ЙНМРПНКЮ : ' + controlName + ' ДКЪ ЯРХКЪ рейяр ОН СЛНКВЮМХЧ МЕ "я"');
    end;
  end;//for
  ClickToolbarButton(GetFromStyleEditor('мхфмхи рскаюп'), 'нРЛЕМЮ');
  result := true;  
except  
  if IsExists(editorWnd) then  
    editorWnd.Close;
  SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_13_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;


