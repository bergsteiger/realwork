{
HLTC129.1.3: оПНБЕПХРЭ, ВРН ОПХ ХГЛЕМЕМХХ МЮЯРПНЕЙ ЬПХТРНБ ЙМНОЙЮЛХ МЮ РСКАЮПЕ <сБЕКХВХРЭ/сЛЕМЭЬХРЭ ЬПХТР> 
ОПНХЯУНДХР ЯХМУПНММНЕ СБЕКХВЕМХЕ ЬПХТРНБ БН БЯЕУ ДЕПЕБЭЪУ:
  HLTC129.1.3.1: б ЮРПХАСРЮУ ДНЙСЛЕМРЮ
}  
function HLTC129_1_3_1_Execute(var  CurrentState: OleVariant): OleVariant;  
var
  toolBar;  
  wnd;
  x, y;
  picture;
  i;
  fontSize: integer;
begin
try
  if NOT SetDefaultSettingsInStyleEditor then
    Raise('мЕ СДЮКНЯЭ СЯРЮМНБХРЭ МЮЯРПНИЙХ Б пЕДЮЙРНПЕ ЯРХКЕИ ОН СЛНКВЮМХЧ');      
  if NOT OpenDocByNumber(10003000) then Raise('МЕ СДЮКНЯЭ НРЙПШРЭ ДНЙСЛЕМР ОН МНЛЕПС'); 
  wnd := OpenInDocument('хмтнплюжхъ н днйслемре');
  if NOT IsExists(wnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ БЙКЮДЙС хмтнплюжхъ н днйслемре');  
  toolBar := GetFromMainWindow('рскаюп');
  if NOT IsExists(toolBar) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ рскаюп');  
  for i := 0 to 4 do
  begin
    if NOT ClickToolBarButton(toolBar, 'сБЕКХВХРЭ ЬПХТР') then
      Raise('мЕ СДЮКНЯЭ ЙКХЙМСРЭ МЮ ЙМНОЙС сБЕКХВХРЭ ЬПХТР');
      
    case i of
      0: fontSize := 13;
      1: fontSize := 14;
      2: fontSize := 15;
      3: fontSize := 16;
      4: fontSize := 17;
    end;//case
    
    picture := RenderTextToPicture('чПХДХВЕЯЙЮЪ', 'Arial CYR', fontSize, '');     
    if NOT FindPicture(
            wnd, 
            picture,
            x, y
    ) then
    begin
      Log.Picture(picture, 'йЮПРХМЙЮ НРПЕМДЕПЕМНЦН ЬПХТРЮ');
      Log.Picture(wnd, 'йЮПРХМЙЮ НАЗЕЙРЮ, МЮ ЙНРНПНЛ ХЫЕРЯЪ ЬПХТР');
    
      Log.Error('мЕ СДЮКНЯЭ Б ЮРПХАСРЮУ ДНЙСЛЕМРЮ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ ПЮГЛЕПЮ =' + IntToStr(fontSize));
      Raise('мЕ СДЮКНЯЭ Б ЮРПХАСРЮУ ДНЙСЛЕМРЮ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ ПЮГЛЕПЮ =' + IntToStr(fontSize));    
    end;  
    
  end;//for     
  
  for i := 0 to 4 do
  begin
    if NOT ClickToolBarButton(toolBar, 'сЛЕМЭЬХРЭ ЬПХТР') then
      Raise('мЕ СДЮКНЯЭ ЙКХЙМСРЭ МЮ ЙМНОЙС сЛЕМЭЬХРЭ ЬПХТР');
      
    case i of
      0: fontSize := 16;
      1: fontSize := 15;
      2: fontSize := 14;
      3: fontSize := 13;
      4: fontSize := 12;
    end;//case
    
    picture := RenderTextToPicture('чПХДХВЕЯЙЮЪ', 'Arial CYR', fontSize, '');     
    if NOT FindPicture(
            wnd, 
            picture,
            x, y
    ) then
    begin
      Log.Picture(picture, 'йЮПРХМЙЮ НРПЕМДЕПЕМНЦН ЬПХТРЮ');
      Log.Picture(wnd, 'йЮПРХМЙЮ НАЗЕЙРЮ, МЮ ЙНРНПНЛ ХЫЕРЯЪ ЬПХТР');
    
      Log.Error('мЕ СДЮКНЯЭ Б ЮРПХАСРЮУ ДНЙСЛЕМРЮ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ ПЮГЛЕПЮ =' + IntToStr(fontSize));
      Raise('мЕ СДЮКНЯЭ Б ЮРПХАСРЮУ ДНЙСЛЕМРЮ МЮИРХ ТПЮЦЛЕМР РЕЙЯРЮ Я щрюкнммшл ьПХТРНЛ ПЮГЛЕПЮ =' + IntToStr(fontSize));    
    end;  
    
  end;//for
  
  result := true;  
except
  SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_3_1_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;

