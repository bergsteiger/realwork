  SetDefaultSettingsInStyleEditor;  
  result := true;  
except
  SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_3_7_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;

{
HLTC129.1.4: сАЕДХРЭЯЪ, ВРН ДКЪ НАНГМЮВЕМХЪ РХОЮ МЮЯКЕДНБЮМХЪ ХЯОНКЭГСЕРЯЪ ЙМНОЙЮ БШАНПЮ 
(п, я ХКХ о), ПЮЯОНКНФЕММЮЪ ЯОПЮБЮ НР МЮГБЮМХЪ ЯННРБЕРЯРБСЧЫЕЦН ОНКЪ (мЮГБЮМХЕ, рЕЙЯР, тНМ). 
оПНБЕПХРЭ, ВРН ЫЕКВЙНЛ ЛШЬХ ОН ДЮММНИ ЙМНОЙЕ ГЮДЮЧРЯЪ:
  HLTC129.1.4.1: гЮХЛЯРБНБЮМХЕ ГМЮВЕМХИ ОЮПЮЛЕРПНБ, ОПХЯСЫХУ ЯРХКЧ-ПНДХРЕКЧ - п
}  
function HLTC129_1_4_1_Execute(var  CurrentState: OleVariant): OleVariant;  
var
  editorWnd;
  lable; lableName;
  button; buttonName;
  current;
  toolBar;
  i, j;
begin
try
  editorWnd := GoToStyleEditor;
  if NOT IsExists(editorWnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ ПЕДЮЙРНП ЯРХКЕИ');
  if NOT Position_Internal(GetFromStyleEditor('депебн ярхкеи'), 'рейяр\жбернбне бшдекемхе', '\', true, 'IsPressHome:=true' ) then
    Raise('мЕ СДЮКНЯЭ ЯОНГХЖХНМХПНБЮХРЭЯЪ МЮ ЯРХКЭ: рейяр\жбернбне бшдекемхе');
  for i := 1 to 3 do
  begin
    case i of
      1: begin lableName := 'кщиак "мюгбюмхе"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "мюгбюмхе"'; end;
      2: begin lableName := 'кщиак "жбер"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "жбер"'; end;
      3: begin lableName := 'кщиак "тнм"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "тнм"'; end;
    end;  
    lable := GetFromStyleEditor(lableName);
    if NOT IsExists(lable) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ :' + lableName);
    button := GetFromStyleEditor(buttonName);
    if NOT IsExists(button) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ :' + buttonName);
    
    j := 0;
    while ( common.GetWindowText(button.Handle) <> 'я' ) do
    begin
      button.Click;
      inc(j);
      if j = 3 then Raise('мЕ СДЮКНЯЭ ДКЪ ЙМНОЙХ "' + buttonName + '" СЯРЮМНБХРЭ ГМЮВЕМХЕ "я"');       
    end;
    button.Click;
    current := common.GetWindowText(button.Handle); 
    if current <> 'п' then
      Raise('яКЕДНЛ ГЮ ГМЮВЕМХЕЛ "я" ДНКФМН ХДРХ "п", Ю ЯЕИВЮЯ ХД╦Р "' + current + '"');      
    button.Click;
    current := common.GetWindowText(button.Handle); 
    if current <> 'о' then
      Raise('яКЕДНЛ ГЮ ГМЮВЕМХЕЛ "п" ДНКФМН ХДРХ "о", Ю ЯЕИВЮЯ ХД╦Р "' + current + '"');  
    button.Click;
    current := common.GetWindowText(button.Handle); 
    if current <> 'я' then
      Raise('яКЕДНЛ ГЮ ГМЮВЕМХЕЛ "о" ДНКФМН ХДРХ "я", Ю ЯЕИВЮЯ ХД╦Р "' + current + '"');
      
    if ( ( (button.Top + button.Height) + 2) < (lable.Top + lable.Height)) OR ( ( (button.Top + button.Height) - 2 ) > (lable.Top + lable.Height)) then
    begin
      Log.Error('йМНОЙЮ Х кЩИАК МЕ МЮУНДЪРЯЪ МЮ НДМНЛ СПНБМЕ(+\-2 ОХЙЯЕКЪ) ОН БЕПРХЙЮКХ');
      Raise('йМНОЙЮ Х кЩИАК МЕ МЮУНДЪРЯЪ МЮ НДМНЛ СПНБМЕ(+\-2 ОХЙЯЕКЪ) ОН БЕПРХЙЮКХ');
    end;
        
    if button.Left <= lable.Left then
    begin
      Log.Error('йМНОЙЮ МЕ МЮУНДХРЯЪ ЯОПЮБЮ НР кЩИАКЮ');
      Raise('йМНОЙЮ МЕ МЮУНДХРЯЪ ЯОПЮБЮ НР кЩИАКЮ');
    end;
    
  end;//for
  
  toolBar := GetFromStyleEditor('мхфмхи рскаюп');
  if NOT IsExists(toolBar) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ мхфмхи рскаюп');
  if NOT ClickToolbarButton(toolBar, 'нРЛЕМЮ') then 
      Raise('мЕ СДЮКНЯЭ МЮФЮРЭ "нРЛЕМЮ" МЮ МХФМЕЛ РСКАЮПЕ ДЕПЕБЮ ЯРХКЕИ!');
    WaitForConfirmationWindow(2000, 'дЮ');
  SetDefaultSettingsInStyleEditor;
                                           
  result := true;  
except
  if IsExists(editorWnd ) then editorWnd.Close;
  WaitForConfirmationWindow(2000, 'дЮ');    
  SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_4_1_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;

{
HLTC129.1.5: оПНБЕПХРЭ, ВРН ОПХ ХГЛЕМЕМХХ РХОЮ МЮЯКЕДНБЮМХЪ ЯННРБЕРЯРБЕММН ЛЕМЪЕРЯЪ ЙНМРПНК 
(ЕЦН ЯНДЕПФХЛНЕ) НРБЕВЮЧЫХИ ГЮ МЮЯРПНИЙС ЩРНЦН ОЮПЮЛЕРПЮ.
}  
function HLTC129_1_5_Execute(var  CurrentState: OleVariant): OleVariant;  
var
  editorWnd;  
  button, buttonName;
  control, controlName,  
  toolBar;
  i, j;
  valueDef, addValueDef;
  valueColor;
begin
try
  editorWnd := GoToStyleEditor;
  if NOT IsExists(editorWnd) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ ПЕДЮЙРНП ЯРХКЕИ');
  if NOT Position_Internal(GetFromStyleEditor('депебн ярхкеи'), 'рейяр\жбернбне бшдекемхе', '\', true, 'IsPressHome:=true' ) then
    Raise('мЕ СДЮКНЯЭ ЯОНГХЖХНМХПНБЮХРЭЯЪ МЮ ЯРХКЭ: рейяр\жбернбне бшдекемхе');
  for i := 1 to 8 do
  begin
    case i of
      1: begin controlName := 'йнлананйя "мюгбюмхе"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "мюгбюмхе"'; valueDef := 'Arial CYR'; end;
      2: begin controlName := 'йнлананйя "жбер"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "жбер"'; valueDef := 3090470; addValueDef:=ClBlack; end;
      3: begin controlName := 'йнлананйя "тнм"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "тнм"'; valueDef:=ClWhite; addValueDef:=ClWhite; end;
      4: begin controlName := 'йнлананйя "пюглеп"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "пюглеп"'; valueDef:='13'; end;
      5: begin controlName := 'вейанйя "фхпмши"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "фхпмши"'; valueDef:=true; end;
      6: begin controlName := 'вейанйя "мюйкнммши"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "мюйкнммши"'; valueDef:=false; end;
      7: begin controlName := 'вейанйя "ондв╗пймсрши"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "ондв╗пймсрши"'; valueDef:=false; end;
      8: begin controlName := 'вейанйя "оепев╗пймсрши"'; buttonName := 'ймнойю рхою мюякеднбюмхъ "оепев╗пймсрши"'; valueDef:=false; end;
    end;
      
    button := GetFromStyleEditor(buttonName);
    if NOT IsExists(button) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ :' + buttonName);
    //-------------я
    j := 0;
    while ( common.GetWindowText(button.Handle) <> 'я' ) do
    begin
      button.Click;
      inc(j);
      if j = 3 then Raise('мЕ СДЮКНЯЭ ДКЪ ЙМНОЙХ "' + buttonName + '" СЯРЮМНБХРЭ ГМЮВЕМХЕ "я"');       
    end;
    control := GetFromStyleEditor(controlName);
    if NOT IsExists(control) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ :' + controlName);    
    case AnsiUpperCase(control.WndClass) of    
      'TVTCOMBOBOXQS':      
        if NOT ( (control.Enabled) AND (common.GetWindowText(control.Handle) = valueDef) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "я" ');    
      'TVTCOLORBOX':
      begin
        valueColor := control.Selected;
        if ( valueColor = clDefault ) then
          valueColor := control.Picture(control.Height div 2, control.Height div 2, 2, 2).Pixels(0, 0);
      
        if NOT ( (control.Enabled) AND (valueColor = valueDef) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "я", ДНКФМН АШРЭ : ' +
            IntToStr(valueDef)+ ' Ю С МЮЯ : ' + IntToStr(valueColor) );
      end;                        
      'TVTCHECKBOX':
        if NOT ( (control.Enabled) AND (VarToBool(control.wState) = valueDef) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "я" ');
      else
        Raise('мЕХГБЕЯРМШИ ЙКЮЯЯ ЙНМРПНКЮ (' + control.WndClass +') !!!');    
    end;//case
    //-------------о
    j := 0;
    while ( common.GetWindowText(button.Handle) <> 'о' ) do
    begin
      button.Click;
      inc(j);
      if j = 3 then Raise('мЕ СДЮКНЯЭ ДКЪ ЙМНОЙХ "' + buttonName + '" СЯРЮМНБХРЭ ГМЮВЕМХЕ "о"');       
    end;    
    case AnsiUpperCase(control.WndClass) of    
      'TVTCOMBOBOXQS':      
        if NOT ( (NOT control.Enabled) AND (common.GetWindowText(control.Handle) = '') ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "о" ');    
      'TVTCOLORBOX':
      begin
        valueColor := control.Selected;
        if ( valueColor = clDefault ) then
          valueColor := control.Picture(control.Height div 2, control.Height div 2, 2, 2).Pixels(0, 0);
      
        if NOT ( (NOT control.Enabled) AND (valueColor = clSystemColor) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "о", ДНКФМН АШРЭ : ' +
            IntToStr(valueDef)+ ' Ю С МЮЯ : ' + IntToStr(valueColor) );
      end;                        
      'TVTCHECKBOX':
        if NOT ( (NOT control.Enabled) AND (NOT control.Visible) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "о" ');
      else
        Raise('мЕХГБЕЯРМШИ ЙКЮЯЯ ЙНМРПНКЮ (' + control.WndClass +') !!!');    
    end;//case
    
    //-------------п
    j := 0;
    while ( common.GetWindowText(button.Handle) <> 'п' ) do
    begin
      button.Click;
      inc(j);
      if j = 3 then Raise('мЕ СДЮКНЯЭ ДКЪ ЙМНОЙХ "' + buttonName + '" СЯРЮМНБХРЭ ГМЮВЕМХЕ "п"');       
    end;    
    case AnsiUpperCase(control.WndClass) of    
      'TVTCOMBOBOXQS':      
        if NOT ( (NOT control.Enabled) AND (common.GetWindowText(control.Handle) = valueDef) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "п" ');    
      'TVTCOLORBOX':
      begin
        valueColor := control.Selected;
        if ( valueColor = clDefault ) then
          valueColor := control.Picture(control.Height div 2, control.Height div 2, 2, 2).Pixels(0, 0);
      
        if NOT ( (NOT control.Enabled) AND (control.Visible) AND(valueColor = addValueDef) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "п", ДНКФМН АШРЭ : ' +
            IntToStr(valueDef)+ ' Ю С МЮЯ : ' + IntToStr(valueColor) );
      end;                        
      'TVTCHECKBOX':
        if NOT ( (NOT control.Enabled) AND (control.Visible) AND (control.Checked = false) ) then        
          Raise('йНМРПНК ' + controlName + ' МЕ Б ЩРЮКНММНЛ ЯНЯРНЪМХХ ОПХ РХОЕ МЮЯКЕДНБЮМХЪ "п" ');
      else
        Raise('мЕХГБЕЯРМШИ ЙКЮЯЯ ЙНМРПНКЮ (' + control.WndClass +') !!!');    
    end;//case
    
  end;//for
  
  toolBar := GetFromStyleEditor('мхфмхи рскаюп');
  if NOT IsExists(toolBar) then Raise('мЕ СДЮКНЯЭ ОНКСВХРЭ мхфмхи рскаюп');
  if NOT ClickToolbarButton(toolBar, 'нРЛЕМЮ') then 
      Raise('мЕ СДЮКНЯЭ МЮФЮРЭ "нРЛЕМЮ" МЮ МХФМЕЛ РСКАЮПЕ ДЕПЕБЮ ЯРХКЕИ!');
  WaitForConfirmationWindow(2000, 'дЮ');
  SetDefaultSettingsInStyleEditor;
                                           
  result := true;  
except
  if IsExists(editorWnd ) then editorWnd.Close;
  WaitForConfirmationWindow(2000, 'дЮ');    
  //SetDefaultSettingsInStyleEditor;
  Log.Message( 'HLTC129_1_5_Execute: ' + ExceptionMessage , '' , pmNormal, GetLogAttr('EXCEPTION'));
  result := false;
  CommonExceptionHandler;
end;
end;

