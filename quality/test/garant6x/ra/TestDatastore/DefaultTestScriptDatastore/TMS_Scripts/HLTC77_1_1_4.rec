'$Include "global.sbl"
'$include "States.sbl"
Dim ListArray(1 to 1) as String
Sub FillArray
    ListArray(1)="Обращение Председателей Центральной комиссии Республики Беларусь по выборам и проведению республиканских референдумов, Центральной избирательной комиссии Республики Казахстан, Центральной комиссии по выборам и проведению референдумов Кыргызской Республики, Центральной избирательной комиссии Российской Федерации"
  
                   
end sub
Sub Main
    call FillArray
    CUR_STATE=ST_NONE_STANDART
    Call NeedState (ST_PPR_6x)
    call Select_Property(gl_Organ_Source,"Центральная избирательная комиссия России",1)
    call Select_Property(gl_Part_Topic,"Основы государственно-правового устройства",1)
    Window Click, "CurrentWindow","Coords=560,185"
    Window SetContext, "Caption={ГАРАН*}", ""
    InputKeys "^{ENTER}"
    Window SetContext, "Caption={ГАРАН*}", ""
    ListBox Click, "ObjectIndex=4", "Coords=8,8"
    Dim ListCount%
    Dim elem$
    SQAGetProperty "CurrentFocus","ItemCount",ListCount
    call StrComp2Log(CSTR(ListCount),"1")
    call CheckItemNames(ListArray,"{DOWN}",0,ListCount,1)
    CUR_STATE=ST_NONE_STANDART 
End Sub
