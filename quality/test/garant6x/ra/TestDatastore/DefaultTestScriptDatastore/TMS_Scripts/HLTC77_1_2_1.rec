'$Include "global.sbl"
'$include "States.sbl"
Dim ListArray(1 to 1) as String
Sub FillArray
    ListArray(1)="Соглашение о мерах по обеспечению улучшения расчетов между хозяйственными организациями стран-участниц Содружества Независимых Государств (Ташкент, 15 мая 1992 г.)"           
end sub
Sub Main
    call FillArray
    CUR_STATE=ST_NONE_STANDART
    Call NeedState (ST_PPS_6x)
    ListBox Click, "ObjectIndex=5", "Coords=53,10"
    InputKeys "{HOME}авизо{ENTER}"
    InputKeys "^{ENTER}"
    Dim ListCount%
    SQAGetProperty "CurrentFocus","ItemCount",ListCount
    call StrComp2Log(CSTR(ListCount),"1")
    call CheckItemNames(ListArray,"{DOWN}",0,ListCount,1)
    'ListBox Click, "ObjectIndex=4", "Index=0;Index=0;Coords=11,32"
    CUR_STATE=ST_NONE_STANDART
End Sub
