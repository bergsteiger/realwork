'$Include "global.sbl"
'$include "States.sbl"
Dim ListArray(1 to 1) as String
Sub FillArray
    ListArray(1)="Постановление Правительства РФ от 31 июля 2001 г. N 569 ""О представлении Президенту Российской Федерации предложения о подписании Соглашения между Российской"
          
end sub
Sub Main
    call FillArray
    'CUR_STATE=ST_NONE_STANDART
    Call NeedState (ST_MAIN_MENU)
    
    Window SetContext, "Caption={ГАРАНТ*}", ""
'    GenericObject Click, "Text=Правовой навигатор", "Coords=80,15"
'    GenericObject Click, "Text=Проекты законов", "Coords=58,15"
    GenericObject DblClick, "Text=Правовой навигатор", ""
    GenericObject DblClick, "Text=Проекты законов", ""
 
    if Position_by_Text("CurrentFocus","Законодательство об отдельных видах деятельности",PressHome:=1)<>0 then
       InputKeys "{ENTER}"
    else
       SQALogMessage sqaFail, "Элемент не найден", "Элемент не найден"
    end if
    Window SetContext, "Caption={ГАРАН*}", ""
    Dim ListCount%
    SQAGetProperty "CurrentFocus","ItemCount",ListCount
    call StrComp2Log(CSTR(ListCount),"1")
    call CheckItemNames(ListArray,"{DOWN}",0,ListCount,1)
    CUR_STATE=ST_NONE_STANDART

End Sub
