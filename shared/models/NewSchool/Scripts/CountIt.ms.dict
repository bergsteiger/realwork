UNIT CountIt.ms.dict

INTEGER FUNCTION .CountIt
  ARRAY IN anArray
 0 >>> Result
 anArray .for> ( 
   IN anItem 
  Inc Result 
 ) 
; // .CountIt

BOOLEAN FUNCTION .NotEmpty
  ARRAY IN anArray
 anArray .CountIt 0 !=
 >>> Result 
; // .NotEmpty

BOOLEAN FUNCTION .IsEmpty
  ARRAY IN anArray
 anArray .CountIt 0 ==
 >>> Result 
; // .IsEmpty
