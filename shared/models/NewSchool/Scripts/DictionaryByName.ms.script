PROGRAM DictionaryByName.ms.script

USES
 ElementsRTTI.ms.dict
;

USES
 GarantMetaModel.ms.dict
;

USES
 CollectElements.ms.dict
;

CollectInAllDictionaries: (
  IN aWord
 aWord .Dependencies
 .filter> ( .IsStereotype st_injects::Dependency )
 .map> .Target
 if ( aWord .IsScriptKeywordsPack ) then
 begin
  .join> ( aWord .Inherits )
 end // ( aWord .IsScriptKeywordsPack )
)
 '.pas.ms.script.inj' '_InjectedElements' OutCollected

CollectInAllDictionaries: (
  IN aWord
 aWord .Implements
 .filter> .IsAtom
)
 '.pas.ms.script.impl' '_Implementors' OutCollected
