  end // aValue IsArray
  else
  begin 
   aValue ToPrintable g_OutFile File:WriteStr
  end // aValue IsArray
 ; // .OutValue
 
 IndentStr g_OutFile File:WriteStr
 aValue .OutValue
 \n g_OutFile File:WriteStr
; // .Out

