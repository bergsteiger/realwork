elem_iterator Diagrams
 Cached:
 (
  RULES
   (
    ( Self .IsSomeView )
    AND ( Self .Viewed Self ?!= )
   ) 
    ( Self .Viewed call.me )
   DEFAULT
    ( Self 'Diagrams' .ElemList ) 
  ; // RULES
 )
 >>> Result
; // Diagrams

