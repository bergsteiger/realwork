;

USES
 ViewSuffix.ms.dict
;

USES
 cPathSep.ms.dict
;

USES
 DiagramsRoot.ms.dict
;

elem_iterator msm:Diagrams
 Cached:
 (
  RULES
   (
    ( Self .IsSomeView )
    AND ( Self .Viewed Self ?!= )
   ) 
    ( Self .Viewed call.me )
   (
    ( Self .IsSomeView ! )
    AND ( Self .Diagrams .NotEmpty ) 
   ) 
    ( Self .Diagrams )
   DEFAULT
    begin
     VAR l_Name
     Self .WordName ViewSuffix .CutSuffix >>> l_Name
     VAR l_DiagramsDictName
     l_Name MEPrefix .CutPrefix >>> l_DiagramsDictName
     l_DiagramsDictName DiagramExt Cat >>> l_DiagramsDictName
     [ DiagramsRoot l_DiagramsDictName ] cPathSep strings:CatSep >>> l_DiagramsDictName
     l_Name DiagramsSuffix Cat >>> l_Name
     VAR l_DigramsHolder 
     WL l_Name l_DiagramsDictName >>> l_DigramsHolder
     RULES
      ( l_DigramsHolder .IsNil )
       nil
      DEFAULT
       begin
        l_DigramsHolder .Diagrams
        VAR l_Diagrams
        >>> l_Diagrams
        //l_DigramsHolder 'Diagrams' l_Diagrams .SetElementVar
        //Self 'Diagrams' l_Diagrams .SetElementVar
        // - сохряняем ссылку себе, чтобы потом пережить перечитывание списка
        l_Diagrams
       end // DEFAULT
     ; // RULES  
    end // DEFAULT 
  ; // RULES
 )
 >>> Result
; // msm:Diagrams

